unset exitecho "Compiling globals.cc"compile  +t +e  globals.ccset  errNo  {status}if {errNo} != 0   echo "Compilation of globals.cc failed:  status = {errNo}"   exit {errNo}endecho "Compiling error.cc"compile  +t +e  error.ccset  errNo  {status}if {errNo} != 0   echo "Compilation of error.cc failed:  status = {errNo}"   exit {errNo}endecho "Compiling cmds1.cc"compile  +t +e  cmds1.ccset  errNo  {status}if {errNo} != 0   echo "Compilation of cmds1.cc failed:  status = {errNo}"   exit {errNo}endecho "Compiling cmds2.cc"compile  +t +e  cmds2.ccset  errNo  {status}if {errNo} != 0   echo "Compilation of cmds2.cc failed:  status = {errNo}"   exit {errNo}endecho "Compiling main.cc"compile  +t +e  main.ccset  errNo  {status}if {errNo} != 0   echo "Compilation of main.cc failed:  status = {errNo}"   exit {errNo}endecho "Linking main, globals, error, cmds1, and cmds2"link main globals error cmds1 cmds2 keep=miniwordset  errNo  {status}if {errNo} != 0   echo "Linking of main, globals, error, cmds1 and cmds2 failed:  status = {errNo}"else   filetype miniword s16   echo "Successfully built MiniWord.  Enter miniword as a command to execute it."end