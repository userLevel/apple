MAKE "C2H5OH [C [H []] [H []] [H []] [C [] [H []] [H []] [O [] [H []]]]]MAKE "H2O [O [H []] [H []]]MAKE "C3O3H8 [C [H []] [C [] [H []] [H []] [O [] [H []]]] [O [] [H []]] [C [] [H []] [H []] [O [] [H []]]]]MAKE "CH4 [C [H []] [H []] [H []] [H []]]TO Draw :moleculeMAKE "balls []RUN :moleculeDrawAtomsENDTO DrawAtomsWHILE NOT EMPTYP :balls [DrawFarthest]ENDTO DrawFarthestLOCAL "smLOCAL "restMAKE "sm FIRST :ballsMAKE "balls BUTFIRST :ballsMAKE "rest []WHILE NOT EMPTYP :balls [IF FIRST FIRST :balls > FIRST :sm [MAKE "rest FPUT :sm :rest MAKE "sm FIRST :balls] [MAKE "rest FPUT FIRST :balls :rest] MAKE "balls BUTFIRST :balls]LOCAL "sMAKE "s ITEM 5 :smSETPC ITEM 4 :smPAINTOVAL [ITEM 2 :sm - :s ITEM 3 :sm + :s ITEM 2 :sm + :s ITEM 3 :sm - :s]MAKE "balls :restENDTO O :b1 :b2IF NOT EMPTYP :b1 [SETPC 1 BK 20 RUN :b1 PU FD 20 PD]IF NOT EMPTYP :b2 [RT 107 SETPC 1 FD 20 RUN :b2 PU BK 20 PD LT 107]AddBall 7 5ENDTO H :b1SETPC 1IF NOT EMPTYP :b1 [FD 20 RUN :b1 PU BK 20 PD]AddBall 13 3ENDTO C :b1 :b2 :b3 :b4IF NOT EMPTYP :b1 [SETPC 1 BK 20 RUN :b1 PU FD 20 PD]IF NOT EMPTYP :b2 [RI 70.5 SETPC 1 FD 20 RUN :b2 PU BK 20 PD RO 70.5]IF NOT EMPTYP :b3 [RLL 120 RI 70.5 SETPC 1 FD 20 RUN :b3 PU BK 20 PD RO 70.5 RLR 120]IF NOT EMPTYP :b4 [RLR 120 RI 70.5 SETPC 1 FD 20 RUN :b4 PU BK 20 PD RO 70.5 RLL 120]AddBall 0 6ENDTO AddBall :color :sizeLOCAL "iMAKE "i ( LIST ZCOR XCOR YCOR :color :size )IF NOT NAMEP "balls [MAKE "balls []]MAKE "balls FPUT :i :ballsEND