{ Write out five Fibonacci numbers }program Fibonacci(output);var   last: integer;              {last number}   current: integer;           {current number}   next: integer;              {new number}beginlast := 0;current := 1;next := last+current;writeln(next);last := current;current := next;next := last+current;writeln(next);last := current;current := next;next := last+current;writeln(next);last := current;current := next;next := last+current;writeln(next);last := current;current := next;next := last+current;writeln(next);last := current;current := next;end.