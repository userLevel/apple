program Rectangles;uses Common, QuickDrawII;var   i:integer;                   {loop variable}beginSetPenMode(0);                  {set up for graphics}SetSolidPenPat(0);SetPenSize(3,1);for i := 10 to 60 do begin      {draw a black rectangle}   MoveTo(10,i);   LineTo(250,i);   end; {for}SetSolidPenPat(1);              {draw a green rectangle}for i := 31 to 49 do begin   MoveTo(220,i);   LineTo(270,i);   end; {for}SetSolidPenPat(0);              {outline it in black}MoveTo(220,30);LineTo(220,50);LineTo(270,50);LineTo(270,30);LineTo(220,30);SetSolidPenPat(2);              {draw a purple rectangle}for i := 41 to 79 do begin   MoveTo(50,i);   LineTo(300,i);   end; {for}SetSolidPenPat(0);              {outline it in black}MoveTo(50,40);LineTo(50,80);LineTo(300,80);LineTo(300,40);LineTo(50,40);end.