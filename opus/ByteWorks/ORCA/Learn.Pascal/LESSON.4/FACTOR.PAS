{ This program prints all of the numbers that divide evenly      }{ into another number.                                           }program Factor(output);const   max = 20;                    {largest number to check}var   i: integer;                  {loop variable}   procedure Factor(n: integer);   { Print the factors of n.                                      }   {                                                              }   { Parameters:                                                  }   {    n - number to check                                       }   var      i: integer;               {loop variable}   begin {Factor}   write(n: 4, ': ');   for i := 2 to n-1 do      if (n mod i) = 0 then         write(i: 4);   writeln;   end; {Factor}beginfor i := 1 to max do   Factor(i);end.