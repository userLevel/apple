{ Exploring div, mod and /}program Divide(input,output);var   n : integer;                       {the "top" number in the division}   d : integer;                       {the "bottom" number in the division}   first,last: integer;               {the first/last number to divide}beginwrite('first       :');               {get the first/last numbers}readln(first);write('last        :');readln(last);write('denominator :');               {get the number to divide by}readln(d);writeln;                              {write the table}writeln('     a     b  a mod b  a div b      a/b');for n := first to last do   writeln(n:6, d:6, n div d:9, n mod d:9, n/d:9:4);end.