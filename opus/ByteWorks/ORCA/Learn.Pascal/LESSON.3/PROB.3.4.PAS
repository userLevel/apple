program Dice(input, output);const   sides = 6;                           {# of sides on the dice}   numDice = 2;                         {# of dice to throw}var   i,j: integer;                        {input value; loop counters}beginwrite('Random number seed =');readln(i);seed(i);writeln;for i := 1 to 20 do begin   for j := 1 to numDice do      write(RandomInteger mod sides + 1);   writeln;   end; {for}end.