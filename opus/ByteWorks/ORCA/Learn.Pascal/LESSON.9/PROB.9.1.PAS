{ String I/O demo }program StringIO(output);var   f: file of string[10];   i: integer;begin{open the file for output}rewrite(f);{write the months to the file}f^ := 'January';        put(f);f^ := 'February';       put(f);f^ := 'March';          put(f);f^ := 'April';          put(f);f^ := 'May';            put(f);f^ := 'June';           put(f);f^ := 'July';           put(f);f^ := 'August';         put(f);f^ := 'September';      put(f);f^ := 'October';        put(f);f^ := 'November';       put(f);f^ := 'December';       put(f);{open the same file for input}reset(f);{read and echo the values}for i := 1 to 12 do begin   writeln(f^);   get(f);   end; {for}end.