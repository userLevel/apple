      REM Draw the extended character set on the graphics screen.      ! Switch to the graphics screen.      HGR      ! Paint the screen white.      FOR R% = 0 TO 199        MOVETO (0, R%)        LINETO (319, R%)      NEXT      ! Draw the characters.      FOR C% = 0 TO 15        FOR R% = 0 TO 15          MOVETO (10 + 12 * C%, 10 + 12 * R%)          PRINT CHR$ (C% * 16 + R%);        NEXT      NEXT      ! Wait for the user to press return.      INPUT "";A$