      REM Add two 3x3 matricies.      DIM A(2, 2), B(2, 2), C(2, 2):! The matricies.      ! Fill in A.      A(0, 0) = 1      A(1, 0) = 2      A(2, 0) = 3      A(0, 1) = 4      A(1, 1) = 5      A(2, 1) = 6      A(0, 2) = 7      A(1, 2) = 8      A(2, 2) = 9      ! Fill in B.      FOR R% = 0 TO 2        FOR C% = 0 TO 2          B(C%, R%) = 1        NEXT      NEXT      ! Add the two matricies.      FOR R% = 0 TO 2        FOR C% = 0 TO 2          C(C%, R%) = A(C%, R%) + B(C%, R%)        NEXT      NEXT      ! Print the result.      FOR R% = 0 TO 2        PRINT C(0, R%), C(1, R%), C(2, R%)      NEXT