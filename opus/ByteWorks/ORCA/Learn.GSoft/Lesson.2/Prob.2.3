      REM Draw five concentric rectangles      DIM TOP AS INTEGER :! top of the rectangle      DIM BOTTOM AS INTEGER :! bottom of the rectangle      DIM LEFT AS INTEGER :! left side of the rectangle      DIM RIGHT AS INTEGER :! right side of the rectangle      DIM I AS INTEGER :! loop/index variable      ! Set up the graphics screen      HGR      SETPENMODE (0)      SETSOLIDPENPAT (15)      ! Initialize the coordinates for the first rectangle      TOP = 10      BOTTOM = 70      LEFT = 10      RIGHT = 100      ! Draw the rectangles      FOR I = 1 TO 5        ! Draw one rectangle        MOVETO (LEFT, TOP)        LINETO (RIGHT, TOP)        LINETO (RIGHT, BOTTOM)        LINETO (LEFT, BOTTOM)        LINETO (LEFT, TOP)        ! Adjust the coordinates for the next rectangle        LEFT = LEFT + 6        RIGHT = RIGHT - 6        TOP = TOP + 6        BOTTOM = BOTTOM - 6      NEXT      ! Wait for a return      INPUT "";A$