      REM Draw three colored rectangles with white outlines.      DIM I AS INTEGER :! Loop variable      ! Set up for graphics.      HGR      SETPENMODE (0)      ! Draw a white rectangle.      SETSOLIDPENPAT (15)      FOR I = 10 TO 120        MOVETO (10, I)        LINETO (250, I)      NEXT      ! Draw a red rectangle.      SETSOLIDPENPAT (7)      FOR I = 61 TO 99        MOVETO (220, I)        LINETO (270, I)      NEXT      ! Outline the red rectangle in white.      SETSOLIDPENPAT (15)      MOVETO (220, 60)      LINETO (220, 100)      LINETO (270, 100)      LINETO (270, 60)      LINETO (220, 60)      ! Draw a blue rectangle.      SETSOLIDPENPAT (4)      FOR I = 81 TO 159        MOVETO (50, I)        LINETO (300, I)      NEXT      ! Outline the blue rectangle in white.      SETSOLIDPENPAT (15)      MOVETO (50, 80)      LINETO (50, 160)      LINETO (300, 160)      LINETO (300, 80)      LINETO (50, 80)      ! Wait for the user to press return.      INPUT "";A$