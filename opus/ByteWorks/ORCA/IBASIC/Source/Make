**  Compile all or part of the compiler*unset exitif {#} == 0  Newer obj/basic.a basic.pas  if {status} != 0     echo basic needs recompilation     set basic basic  end  Newer obj/parser.a parser.pas  if {status} != 0     echo parser needs recompilation     set parser parser     set basic basic  end  Newer obj/codegen.a codegen.pas  if {status} != 0     echo codegen needs recompilation     set codegen codegen     set parser parser  end  Newer obj/scanner.a scanner.pas  if {status} != 0     echo scanner needs recompilation     set scanner scanner     set codegen codegen     set parser parser     set basic basic  end  Newer obj/basiccom.a basiccom.pas  if {status} != 0     echo basiccom needs recompilation     set basiccom basiccom     set codegen codegen     set parser parser     set scanner scanner     set basic basic  endelse  for i    set {i} {i}  endendset exit onset list "{basiccom} {scanner} {codegen} {parser} {basic}"if "{list}" != ""   for i in {list}      echo compile +t +e {i}.pas keep=obj/{i}      compile +t +e {i}.pas keep=obj/{i}   endendecho link obj/basic obj/parser obj/scanner obj/codegen obj/basiccom keep=5/ibasiclink obj/basic obj/parser obj/scanner obj/codegen obj/basiccom keep=5/ibasic