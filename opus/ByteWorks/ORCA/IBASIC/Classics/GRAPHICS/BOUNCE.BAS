1 N = 1010 GR15 COLOR=020 FOR K = 40 TO 47: HLIN 0,39 AT K: NEXT K25 COLOR=930 HLIN 0,39 AT 040 VLIN 0,47 AT 050 HLIN 0,39 AT 4760 VLIN 0,47 AT 3970 X = 7: Y = 980 DX = 1: DY = 190 CNT = 0100 X = X+DX: Y = Y+DY120 COLOR=0: PLOT X-DX,Y-DY130 COLOR=15: PLOT X,Y150 DELX = DX: DELY = DY160 IF SCRN (X+DX,Y) THEN DELX = -DX180 IF SCRN (X,Y+DY) THEN DELY = -DY200 IF SCRN (X+DELX,Y+DELY)=0 THEN 230220 DELX = -DX: DELY = -DY230 IF DX=DELX AND DY=DELY THEN 270240 CALL CLICK: CALL CLICK: CALL CLICK: CALL CLICK: CALL CLICK: CALL CLICK: CALL CLICK: CALL CLICK250 DX = DELX: DY = DELY270 CNT = CNT+1300 COLOR=0310 LOC = 6*RND (6)+6315 HERE = 5*RND (7)+5320 IF NOT SCRN (HERE,LOC+1) THEN COLOR=RND (14)+1325 IF ABS (X-HERE)<=1 THEN 340330 IF CNT=N THEN VLIN LOC,LOC+6 AT HERE340 COLOR=0350 LOC = 5*RND (6)+5360 HERE = 6*RND (7)+6370 IF NOT SCRN (LOC+1,HERE) THEN COLOR=RND (14)+1375 IF ABS (Y-HERE)<=1 THEN 390380 IF CNT=N THEN HLIN LOC,LOC+5 AT HERE390 IF CNT=N THEN CNT = 0400 CALL KEYBOARD: IF KEY<128 THEN 100: CALL CLEARSTROBE410 END