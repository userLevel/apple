mTitleRefShift,     /* flags */   size8                                /* menu item title resource ID */   };resource rMenuItem (size9) {            /* Size 9 menu item */   size9,                               /* menu item ID */   "9","9",                             /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   size9                                /* menu item title resource ID */   };resource rMenuItem (iconStop) {         /* Stop Icon menu item */   iconStop,                            /* menu item ID */   "","",                               /* key equivalents */   18,                                  /* check character */   refIsResource*itemTitleRefShift,     /* flags */   iconStop                             /* menu item title resource ID */   };resource rMenuItem (iconNote) {         /* Note Icon menu item */   iconNote,                            /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   iconNote                             /* menu item title resource ID */   };resource rMenuItem (iconCaution) {      /* Stop Icon menu item */   iconCaution,                         /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   iconCaution                          /* menu item title resource ID */   };resource rMenuItem (iconDisk) {         /* Disk Icon menu item */   iconDisk,                            /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   iconDisk                             /* menu item title resource ID */   };resource rMenuItem (iconSwap) {         /* Swap Icon menu item */   iconSwap,                            /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   iconSwap                             /* menu item title resource ID */   };resource rPString (appleMenu, noCrossBank)  {"@"};resource rPString (fileMenu, noCrossBank)   {"  File  "};resource rPString (editMenu, noCrossBank)   {"  Edit  "};resource rPString (sizeMenu, noCrossBank)   {"  Size  "};resource rPString (iconMenu, noCrossBank)   {"  Icon  "};resource rPString (editUndo, noCrossBank)   {"Undo"};resource rPString (editCut, noCrossBank)    {"Cut"};resource rPString (editCopy, noCrossBank)   {"Copy"};resource rPString (editPaste, noCrossBank)  {"Paste"};resource rPString (editClear, noCrossBank)  {"Clear"};resource rPString (fileOpen, noCrossBank)   {"Open..."};resource rPString (fileClose, noCrossBank)  {"Close"};resource rPString (fileQuit, noCrossBank)   {"Quit"};resource rPString (appleAbout, noCrossBank) {"About Sampler..."};resource rPString (size1, noCrossBank)      {"Size 1"};resource rPString (size2, noCrossBank)      {"Size 2"};resource rPString (size3, noCrossBank)      {"Size 3"};resource rPString (size4, noCrossBank)      {"Size 4"};resource rPString (size5, noCrossBank)      {"Size 5"};resource rPString (size6, noCrossBank)      {"Size 6"};resource rPString (size7, noCrossBank)      {"Size 7"};resource rPString (size8, noCrossBank)      {"Size 8"};resource rPString (size9, noCrossBank)      {"Size 9"};resource rPString (iconStop, noCrossBank)   {"Stop Icon"};resource rPString (iconNote, noCrossBank)   {"Note Icon"};resource rPString (iconCaution, noCrossBank) {"Caution Icon"};resource rPString (iconDisk, noCrossBank)   {"Disk Icon"};resource rPString (iconSwap, noCrossBank)   {"Swap Icon"};/*- Abou