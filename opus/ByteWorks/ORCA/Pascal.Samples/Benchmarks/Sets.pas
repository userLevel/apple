{$keep 'Sets'}{$optimize -1}{--------------------------------------------------------------}{                                                              }{  Sets                                                        }{                                                              }{  Test the speed of basic set operations.                     }{                                                              }{  To get the best performance from the desktop development    }{  environment, be sure to turn debugging off from the         }{  Compile Dialog.  Use the Compile command from the Run menu  }{  to get the compile dialog.                                  }{                                                              }{--------------------------------------------------------------}program Sets(output);label 1;const  iter = 2000;type  color = (red,orange,yellow,green,blue,violet);var  i,sum: integer;  c1: color;  s1: set of color;beginwriteln('Start timing...');for i := 1 to iter do begin  sum := 0;  for c1 := red to violet do    if c1 in [red,green,blue] then      sum := sum+1;  if sum <> 3 then begin    writeln('Failed inclusion test');    goto 1;    end; {if}  s1 := [];  for c1 := red to violet do    s1 := s1 + [c1];  if s1 <> [red..violet] then begin    writeln('Failed union test');    goto 1;    end; {if}  for c1 := red to violet do    s1 := s1 - [c1];  if s1 <> [] then begin    writeln('Failed difference test');    goto 1;    end; {if}  end; {for}writeln('Stop.');1:end.