{$keep 'Float'}{$optimize -1}{--------------------------------------------------------------}{                                                              }{  Float                                                       }{                                                              }{  Test simple floating point operations.                      }{                                                              }{  To get the best performance from the desktop development    }{  environment, be sure to turn debugging off from the         }{  Compile Dialog.  Use the Compile command from the Run menu  }{  to get the compile dialog.                                  }{                                                              }{--------------------------------------------------------------}program float(output);const  const1 = 3.141597;  const2 = 1.7839032e4;  count  = 1000;var  a,b,c: double;  i: integer;begina := const1;b := const2;writeln(count:1,' iterations.');for i := 1 to count do begin  c := a*b;  c := c/a;  c := a*b;  c := c/a;  c := a*b;  c := c/a;  c := a*b;  c := c/a;  c := a*b;  c := c/a;  c := a*b;  c := c/a;  c := a*b;  c := c/a;  end;writeln('Done.',c);end.