MODULE BullsEye;FROM QuickDrawII IMPORT Rect, PenNormal, SetSolidPenPat, PaintOval,  DrawCString, MoveTo;FROM SYSTEM IMPORT ADR;VAR  color:    CARDINAL;  radius:   CARDINAL;  r:        Rect;BEGIN  PenNormal;  color := 1;  FOR radius := 20 TO 1 BY -1 DO    SetSolidPenPat(color);    IF color = 1 THEN      color := 2;    ELSE      color := 1;    END;    WITH r DO      h1 := 160 - radius * 5;      h2 := 160 + radius * 5;      v1 := 42 - radius * 2;      v2 := 42 + radius * 2;    END; (*WITH*)    PaintOval(r);  END; (*FOR*)  MoveTo(10, 90);  DrawCString(ADR('Bulls-eye!'));END BullsEye.