program sort (output);var  i: integer;  a: array[1..10] of integer;procedure ShellSort;var  done: boolean;  i,t: integer;begin {ShellSort}repeat  done := true;  for i := 1 to 9 do    if a[i] > a[i+1] then begin      t := a[i];      a[i] := a[i+1];      a[i+1] := t;      done := false;      end; {if}until done;end; {ShellSort}procedure BubbleSort;var  i,j,t: integer;begin {BubbleSort}for i := 2 to 10 do  if a[i] < a[i-1] then begin    j := i;    while (j > 1) and (a[j] < a[j-1]) do begin      t := a[j];      a[j] := a[j-1];      a[j-1] := t;      j := j-1;      end; {while}    end; {if}end; {BubbleSort}beginfor i := 1 to 10 do  a[i] := 20-i;ShellSort;for i := 1 to 10 do  writeln(i,a[i]);writeln;for i := 1 to 10 do  a[i] := 20-i;BubbleSort;for i := 1 to 10 do  writeln(i,a[i]);end.