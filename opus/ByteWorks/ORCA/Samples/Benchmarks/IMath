      !--------------------------------------------------------------      !      !  Integer Math      !      !  Test the speed of the four basic integer math operations.      !      !--------------------------------------------------------------      CONST ITER% = 10000      DIM A AS INTEGER , B AS INTEGER , C AS INTEGER      DIM D AS INTEGER , E AS INTEGER , F AS INTEGER      DIM I AS INTEGER      PRINT "Start timing..."      B = 1000      C = 10      D = 100      E = 5      F = 10      FOR I = 1 TO ITER%        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F        A = B + C - D * E / F      NEXT      IF A = 960 THEN        PRINT "Stop timing - correct result."      ELSE        PRINT "INCORRECT RESULT."      END IF      END