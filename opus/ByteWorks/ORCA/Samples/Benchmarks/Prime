      DIM FLAGS%(8190)      PRINT "10 iterations"      FOR ITER% = 1 TO 10        COUNT% = 0        FOR I% = 0 TO 8190          FLAGS%(I%) = 1        NEXT I%        FOR I% = 0 TO 8190          IF FLAGS%(I%) THEN            PRIME% = I% + I% + 3            ! PRINT PRIME%            K% = I% + PRIME%            WHILE K% <= 8190              FLAGS%(K%) = 0              K% = K% + PRIME%            WEND            COUNT% = COUNT% + 1          END IF        NEXT I%      NEXT ITER%      PRINT COUNT%;" primes"