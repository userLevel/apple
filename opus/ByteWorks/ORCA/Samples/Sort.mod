MODULE Sort;FROM InOut IMPORT WriteString, WriteInt, WriteLn;VAR  i: INTEGER;  a: ARRAY[1..10] OF INTEGER;PROCEDURE ShellSort;VAR  done: BOOLEAN;  i,t: INTEGER;BEGINREPEAT  done := TRUE;  FOR i := 1 TO 9 DO    IF a[i] > a[i+1] THEN      t := a[i];      a[i] := a[i+1];      a[i+1] := t;      done := FALSE;    END;  END;UNTIL done;END ShellSort;PROCEDURE BubbleSort;VAR  i,j,t: INTEGER;BEGINFOR i := 2 TO 10 DO  IF a[i] < a[i-1] THEN    j := i;    WHILE (j > 1) AND (a[j] < a[j-1]) DO      t := a[j];      a[j] := a[j-1];      a[j-1] := t;      j := j-1;    END;  END;END;END BubbleSort;BEGINFOR i := 1 TO 10 DO  a[i] := 20-i;END;ShellSort;FOR i := 1 TO 10 DO  WriteInt(i, 8);  WriteInt(a[i], 8);  WriteLn;END;WriteLn;FOR i := 1 TO 10 DO  a[i] := 20-i;END;BubbleSort;FOR i := 1 TO 10 DO  WriteInt(i, 8);  WriteInt(a[i], 8);  WriteLn;END;END Sort.