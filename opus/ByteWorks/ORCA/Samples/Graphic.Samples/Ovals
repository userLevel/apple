   10 HGR   20 DIM R AS RECT   30 FOR I = 1 TO 1000   40   R.H1 = RND (1) * 320   50   R.H2 = RND (1) * 320   60   IF R.H2 < R.H1 THEN   70     T% = R.H1   80     R.H1 = R.H2   90     R.H2 = T%  100   END IF  110   R.V1 = RND (1) * 200  120   R.V2 = RND (1) * 200  130   IF R.V2 < R.V1 THEN  140     T% = R.V1  150     R.V1 = R.V2  160     R.V2 = T%  170   END IF  180   SET640COLOR ( RND (1) * 16)  190   PAINTOVAL (R)  200 NEXT  210 GET A$