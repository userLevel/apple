program BullsEye(output);uses Common, QuickDrawII;var  color: integer;  radius: integer;  r: rect;beginPenNormal;color := 1;for radius := 20 downto 1 do begin  SetSolidPenPat(color);  color := color ! 3;  with r do begin    h1 := 160-radius*5;    h2 := 160+radius*5;    v1 := 42-radius*2;    v2 := 42+radius*2;    end; {with}  PaintOval(r);  end; {for}writeln('Bulls-eye!');end.