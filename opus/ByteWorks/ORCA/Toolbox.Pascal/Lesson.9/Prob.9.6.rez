tem title resource ID */   };resource rMenuItem (colorsBrown) {      /* Brown menu item */   colorsBrown,                         /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   colorsBrown                          /* menu item title resource ID */   };resource rMenuItem (colorsRed) {        /* Red menu item */   colorsRed,                           /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   colorsRed                            /* menu item title resource ID */   };resource rMenuItem (colorsOrange) {     /* Orange menu item */   colorsOrange,                        /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   colorsOrange                         /* menu item title resource ID */   };resource rMenuItem (colorsYellow) {     /* Yellow menu item */   colorsYellow,                        /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   colorsYellow                         /* menu item title resource ID */   };resource rMenuItem (colorsGreen) {      /* Green menu item */   colorsGreen,                         /* menu item ID */   "","",                               /* key equivalents */   0,                                   /* check character */   refIsResource*itemTitleRefShift,     /* flags */   colorsGreen                          /* menu item title resource ID */   };resource rMenuItem (colorsBlue) {       /* Blue menu item */   colorsBlue,   