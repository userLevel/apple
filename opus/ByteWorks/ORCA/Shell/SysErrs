#define APWSysErr   'gs'        /* resource type (gs for AppleIIgs) */#define APWSysErrID 'APW'       /* resource ID base number */#define GSOS        0x0000#define Locator     0x0001#define MemMgr      0x0002#define MiscTool    0x0003#define QuickDraw   0x0004#define DeskMgr     0x0005#define EventMgr    0x0006#define Scheduler   0x0007#define SoundMgr    0x0008#define ADB         0x0009#define SANE        0x000a#define IntMath     0x000b#define TextTool    0x000c#define RAMTool     0x000d#define WindowMgr   0x000e#define MenuMgr     0x000f#define ControlMgr  0x0010#define Loader      0x0011#define QDAux       0x0012#define PrintMgr    0x0013#define LineEdit    0x0014#define DialogMgr   0x0015#define ScrapMgr    0x0016#define StdFile     0x0017#define NoteSynth   0x0019#define NoteSeq     0x001a#define FontMgr     0x001b#define ListMgr     0x001c#define ACE         0x001d#define ResMgr      0x001e#define MIDI        0x0020#define TextEdit    0x0022type APWSysErr {    /* APW Standard Error Message Resource Format */    pstring;            /* toolset name */    integer = $$Countof(StringArray);    array StringArray {        hex integer;    /* the numerical error code */        pstring;        /* produce a pascal type string */    };};resource APWSysErr (APWSysErrID + GSOS) {  /* GS/OS error codes */    "GS/OS",    {        0x0001, "Bad GS/OS call number",        0x0004, "Parameter count out of range",        0x0007, "GS/OS is busy",        0x0010, "Device not found",        0x0011, "Invalid device number",        0x0020, "Invalid request",        0x0021, "Invalid control or status code",        0x0022, "Invalid parameter",        0x0023, "Character device not open",        0x0024, "Character device already open",        0x0025, "Interrupt vector table full",        0x0026, "Resource not available",        0x0027, "I/O error",        0x0028, "No device connected",        0x0029, "Device is busy",        0x002b, "Disk is write protected",        0x002c, "Invalid byte count",        0x002d, "Invalid block number",        0x002e, "Disk switched",        0x002f, "Device off-line",        0x0040, "Invalid pathname syntax",        0x0043, "Invalid reference number",        0x0044, "Path not found",        0x0045, "Volume not found",        0x0046, "File not found",        0x0047, "Duplicate pathname",        0x0048, "Volume full",        0x0049, "Volume directory full",        0x004a, "Version error",        0x004b, "Unsupported storage type",        0x004c, "EOF encountered",        0x004d, "Position out of range",        0x004e, "Invalid access",        0x004f, "Buffer too small",        0x0050, "File is busy",        0x0051, "Directory error",        0x0052, "Unsupported volume type",        0x0053, "Invalid parameter",        0x0054, "Out of memory",        0x0057, "Duplicate volume",        0x0058, "Not a block device",        0x0059, "Invalid file level",        0x005a, "Block number out of range",        0x005b, "Invalid path names",        0x005c, "Not a system file",        0x005d, "File system not supported",        0x005f, "Stack overflow",        0x0060, "Data is unavailable",        0x0061, "End of directory",        0x0062, "Invalid class",        0x0063, "Resource fork not found",        0x0067, "Duplicate device name",        0x0070, "Resource fork already exists",        0x0071, "Can't add resource fork"    };};resource APWSysErr (APWSysErrID + Locator) {  /* Tool Locator error codes */    "Tool Locator",    {        0x0001, "Specified tool set not found",        0x0002, "Specified routine not found",        0x0110, "Specified minimum version not found",        0x0111, "Specified message not found"    };};resource APWSysErr (APWSysErrID + MemMgr) {  /* Memory Manager error codes */    "Memory Manager",    {        0x0201, "Unable to allocate block",        0x0202, "Illegal operation on an empty handle",        0x0203, "Empty handle expected for this operation",        0x0204, "Illegal operation on a locked or immovable block",        0x0205, "Attempt to purge an unpurgeable block",        0x0206, "Invalid handle",        0x0207, "Invalid user ID",        0x0208, "Illegal operation with specified attributes"    };};resource APWSysErr (APWSysErrID + MiscTool) {  /* Miscellaneous Tool Set error codes */    "Miscellaneous Tools",    {        0x0301, "Bad input parameter",        0x0302, "No device for input parameter",        0x0303, "Specified task already in Heartbeat queue",        0x0304, "No signature detected in task header",        0x0305, "Damaged Heartbeat queue detected",        0x0306, "Specified task not in queue",        0x0307, "Unsuccessful firmware task",        0x0308, "Damaged HeartBeat queue detected",        0x0309, "Dispatch attempted to unconnected device",        0x030b, "No ID tag available"    };};resource APWSysErr (APWSysErrID + QuickDraw) {  /* QuickDraw error codes */    "QuickDraw",    {        0x0401, "QuickDraw II already initialized",        0x0402, "Cannot reset",        0x0403, "QuickDraw II not initialized",        0x0410, "Screen reserved",        0x0411, "Invalid rectangle specified",        0x0420, "Chunkiness not equal",        0x0430, "Region already open",        0x0431, "Region not open",        0x0432, "Region scan overflow",        0x0433, "Region full",        0x0440, "Polygon already open",        0x0441, "Polygon not open",        0x0442, "Polygon too big",        0x0450, "Invalid color table number",        0x0451, "Invalid color number",        0x0452, "Invalid scan line number"    };};resource APWSysErr (APWSysErrID + DeskMgr) {  /* Desk Manager error codes */    "Desk Manager",    {        0x0510, "Specified DA not available",        0x0511, "Window pointer is not a pointer to a window owned by an NDA"    };};resource APWSysErr (APWSysErrID + EventMgr) {  /* Event Manager error codes */    "Event Manager",    {        0x0601, "EMStartUp already called",        0x0602, "Can't reset Event Manager",        0x0603, "Event Manager not active",        0x0604, "Event code is greater than 15",        0x0605, "Button number specified is not 0 or 1",        0x0606, "Size of event queue is greater than 3639",        0x0607, "Insufficient memory available for queue",        0x0681, "Event queue damaged--fatal system error",        0x0682, "Queue handle damaged--fatal system error"    };};resource APWSysErr (APWSysErrID + SoundMgr) {  /* Sound Tools error codes */    "Sound Tools",    {        0x0810, "No DOC or RAM found",        0x0811, "DOC address range error",        0x0812, "No SoundStartUp call made",        0x0813, "Invalid generator number",        0x0814, "Synthesizer mode error",        0x0815, "Generator already in use",        0x0817, "Master IRQ not assigned",        0x0818, "Sound Tool Set already started",        0x08ff, "Unclaimed sound interrupt error"    };};resource APWSysErr (APWSysErrID + ADB) {  /* ADB error codes */    "Apple Desktop Bus",    {        0x0910, "Command not completed",        0x0911, "Can't synchronize with system",        0x0982, "ADB busy (command pending)",        0x0983, "Device not present at address",        0x0984, "SRQ list full"    };};resource APWSysErr (APWSysErrID + IntMath) {  /* Integer math error codes */    "Integer Math Tool Set",    {        0x0b01, "Bad input parameter",        0x0b02, "Illegal character in string",        0x0b03, "Integer or Longint overflow",        0x0b04, "String overflow"    };};resource APWSysErr (APWSysErrID + TextTool) {  /* Text Tool Set error codes */    "Text Tool Set",    {        0x0c01, "Illegal device type",        0x0c02, "Illegal device number",        0x0c03, "Illegal operation",        0x0c04, "Undefined hardware error",        0x0c05, "Lost device: device no longer on-line",        0x0c06, "File no longer in diskette directory",        0x0c07, "Illegal filename",        0x0c08, "Insufficient space on specified diskette",        0x0c09, "Specified volume not online",        0x0c0a, "Specified file not in directory of specified volume",        0x0c0b, "Duplicate filename",        0x0c0c, "Attempt to open file that is already open",        0x0c0d, "Attempt to access a closed file",        0x0c0e, "Error in reading real or integer number",        0x0c0f, "Ring buffer overflow",        0x0c10, "Specified diskette is write-protected",        0x0c40, "Device error"    };};resource APWSysErr (APWSysErrID + WindowMgr) {  /* Window Manager error codes */    "Window Manager",    {        0x0e01, "First word of parameter is the wrong size",        0x0e02, "Unable to allocate a window record",        0x0e03, "Reserved bits not clear in wmTaskMask field of WmTaskRec",    };};resource APWSysErr (APWSysErrID + ControlMgr) {  /* Control Manager error codes */    "Control Manager",    {        0x1001, "Window Manager not initialized"    };};resource APWSysErr (APWSysErrID + Loader) {     /* System Loader error codes */    "System Loader",    {        0x1101, "Entry not found",        0x1102, "Incompatible OMF version",        0x1103, "Pathname error",        0x1104, "File is not a load file",        0x1105, "Loader is busy",        0x1107, "File version error",        0x1108, "User ID error",        0x1109, "SegNum out of sequence",        0x110a, "Illegal load record found",        0x110b, "Load segment is foreign"    };};resource APWSysErr (APWSysErrID + PrintMgr) {  /* Print Manager error codes */    "Print Manager",    {        0x1301, "Specified driver not in the DRIVERS subdirectory",        0x1302, "Specified port not selected in the control panel",        0x1303, "No print record specified",        0x1304, "Incompatible version of LaserPrep file loaded in LaserWriter",        0x1305, "Incompatible version of LaserPrep file found in DRIVERS directory",        0x1306, "Connection can't be established with the LaserWriter",        0x1307, "Read/write error on the LaserWriter",        0x1321, "LLDStartup call already made",        0x1322, "Invalid control value specified"    };};resource APWSysErr (APWSysErrID + LineEdit) {  /* Line Edit error codes */    "Line Edit",    {        0x1401, "LEStartUp already called",        0x1402, "Can't reset LineEdit",        0x1403, "LineEdit not active",        0x1404, "Desk scrap too big to copy"    };};resource APWSysErr (APWSysErrID + DialogMgr) {  /* Dialog Manager error codes */    "Dialog Manager",    {        0x150a, "Inappropriate item type",        0x150b, "Item creation failed",        0x150c, "No such item",        0x150d, "Frontmost window not a modal dialog window"    };};resource APWSysErr (APWSysErrID + ScrapMgr) {  /* Scrap Manager error codes */    "Scrap Manager",    {        0x1610, "No scrap of this type"    };};resource APWSysErr (APWSysErrID + FontMgr) {  /* Font Manager error codes */    "Font Manager",    {        0x1b01, "FMStartUp call already made",        0x1b02, "Can't reset the Font Manager",        0x1b03, "Font Manager not active",        0x1b04, "Family not found",        0x1b05, "Font not found",        0x1b06, "Font not in memory",        0x1b07, "System font cannot be purgeable",        0x1b08, "Illegal family number",        0x1b09, "Illegal font size",        0x1b0a, "Illegal name length",        0x1b0b, "FixFontMenu never called",        0x1b0c, "Scaled size of font exceeds limits"    };};resource APWSysErr (APWSysErrID + ResMgr) { /* Resource Manager error codes */    "Resource Manager",    {        0x1e01, "Resource fork not empty",        0x1e02, "Format of the resource fork is unknown",        0x1e03, "No converter logged in for resource",        0x1e04, "There are no current (open) resource files",        0x1e05, "ID is already in use",        0x1e06, "Resource was not found",        0x1e07, "Resource file was not found",        0x1e08, "User ID was not found, caller has not called ResourceStartup",        0x1e09, "A unique ID was not found",        0x1e0a, "Index is out of range",        0x1e0b, "System file is already open",        0x1e0c, "Resource is marked as changed, operation cannot be performed",        0x1e0d, "Different converter logged in for the given resource type"    };};resource APWSysErr (APWSysErrID + TextEdit) {   /* TextEdit error codes */    "TextEdit",    {        0x2201, "TextEdit has already been started",        0x2202, "TextEdit has not been started yet",        0x2203, "The TERecord handle was not a handle to a valie TextEdit record",        0x2204, "The descriptor that was passed is not supported by this call",        0x2205, "The flag that was passed was invalid",        0x2206, "The parameter count passed to the call was invalid",        0x2207, "The view rectangle was not valid (less than 20 pixels wide)",        0x2208, "The buffer was too small.  It is still filled with whatever text will fit",        0x2209, "The line number that was passed was too large",        0x220a, "This call is not allowed on the current TextEdit record",        0x220b, "Some parameter that was passed was invalid"    };};