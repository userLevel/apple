(*$Keep 'IMath'*)(*--------------------------------------------------------------*)(*                                                              *)(*  Integer Math                                                *)(*                                                              *)(*  Test the speed of the four basic integer math operations.   *)(*                                                              *)(*  To get the best performance from the desktop development    *)(*  environment, be sure to turn debugging off from the         *)(*  Compile Dialog.  Use the Compile command from the Run menu  *)(*  to get the compile dialog.                                  *)(*                                                              *)(*--------------------------------------------------------------*)MODULE IMath;FROM InOut IMPORT WriteString, WriteLn;CONST  iter = 10000;VAR  a,b,c,d,e,f: INTEGER;  i: INTEGER;BEGIN  WriteString('Start timing...');  WriteLn;  b := 1000;  c := 10;  d := 100;  e := 5;  f := 10;  FOR i := 1 TO iter DO    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;    a := b+c-d*e DIV f;  END;  IF a = 960 THEN    WriteString('Stop timing - correct result.');    WriteLn;  ELSE    WriteString('INCORRECT RESULT.');    WriteLn;  END;END IMath.