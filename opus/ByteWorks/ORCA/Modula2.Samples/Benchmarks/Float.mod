(*$Keep 'Float'*)(*--------------------------------------------------------------*)(*                                                              *)(*  Float                                                       *)(*                                                              *)(*  Test simple floating point operations.                      *)(*                                                              *)(*  To get the best performance from the desktop development    *)(*  environment, be sure to turn debugging off from the         *)(*  Compile Dialog.  Use the Compile command from the Run menu  *)(*  to get the compile dialog.                                  *)(*                                                              *)(*--------------------------------------------------------------*)MODULE float;FROM InOut IMPORT WriteString, WriteLn, WriteInt, WriteLongReal;CONST  const1 = 3.141597;  const2 = 1.7839032E4;  count  = 1000;VAR  a,b,c: LONGREAL;  i: INTEGER;BEGIN  a := const1;  b := const2;  WriteInt(count, 1);  WriteString(' iterations.');  WriteLn;  FOR i := 1 TO count DO    c := a*b;    c := c/a;    c := a*b;    c := c/a;    c := a*b;    c := c/a;    c := a*b;    c := c/a;    c := a*b;    c := c/a;    c := a*b;    c := c/a;    c := a*b;    c := c/a;  END;  WriteString('Done:');  WriteLongReal(c, 16);  WriteLn;END float.