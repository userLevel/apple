f80003300000301f06c00000f0000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f0000030f8000020f8000020f8000020f8000020f80003300000301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000300bfbff00fad0aa03b00000300bfbff00fad0aa03b00000301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f0000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f0000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c0000198000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f8000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f8000030f8000020f8000020f8000020f8000020f80003300000301f06c0000018000030f8000020f8000020f8000020f8000020f80003300000301f06c0000018000030f8000020f8000020f8000020f8000020f80003300000301f06c00000f0000030f8000020f8000020f8000020f8000020f800033ffffff01f06c00000f0000030f8000020f8000020f8000020f8000020f800033ffffff01d00c0fc000030f8000020f8000020f8000020f8000020f80003300000301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000300bfbff00fad0aa03b03ff0300bfbff00fad0aa03b03ff0301a00c0fc000030f8000020f8000020f8000020f8000020f800fd301a00c0fc000030f8000020f8000020f8000020f8000020f800fd301c06c0000607800030f8000020f8000020f8000020f8000020f800fd301c06c0000607800030f8000020f8000020f8000020f8000020f800fd301c06c0001e09800030f8000020f8000020f8000020f8000020f800fd301c06c0001e09800030f8000020f8000020f8000020f8000020f800fd301f06c0000619800030f8000020f8000020f8000020f8000020f8000333f03f301f06c0000619800030f8000020f8000020f8000020f8000020f8000333f03f301f06c0000619800030f8000020f8000020f8000020f8000020f8000330c00c301f06c0000619800030f8000020f8000020f8000020f8000020f8000330c00c301c06c0000619800030f8000020f8000020f8000020f8000020f800fd301c06c0000619800030f8000020f8000020f8000020f8000020f800fd301f06c0000619000030f8000020f8000020f8000020f8000020f80003300cc0301f06c0000619000030f8000020f8000020f8000020f8000020f80003300cc0301f06c0001f9e000030f8000020f8000020f8000020f8000020f80003300300301f06c0001f9e000030f8000020f8000020f8000020f8000020f80003300300301d00c0fc000030f8000020f8000020f8000020f8000020f80003300000301d00c0fc000030f8000020f8000020f8000020f8000020f800033000003004c6ff00f004c6ff00f01206fff000001ffffcd30006600000300000301206fff000001ffffcd30006600000300000301206fff003801f000cd30006600e0033ff00301206fff003801f000cd30006600e0033ff00301206fff00d801f000cd30006600d80330300301206fff00d801f000cd30006600d80330300301206fff031ff1f000cd3000667fc603303ff301206fff031ff1f000cd3000667fc603303ff301206fff0c0031f000cd30006660018330303301206fff0c0031f000cd30006660018330303301206fff300031f000cd3000666000633ff03301206fff300031f000cd3000666000633ff03301206fff0c0031f000cd30006660018306003301206fff0c0031f000cd30006660018306003301206fff031ff1f000cd3000667fc60306003301206fff031ff1f000cd3000667fc60306003301206fff00d801f000cd30006600d80306003301206fff00d801f000cd30006600d80306003301206fff003801f000cd30006600e00307fff301206fff003801f000cd30006600e00307fff301206fff000001ffffcd30006600000300000301206fff000001ffffcd300066000003000003004c6ff00f004c6ff00f0a0008fa00083ff}}{\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 Split screen}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 Split screen}}}{\fs20 s let you split the spreadsheet into pieces, both horizontally and vertically.\par For example, in a checkbook, you would put a row of header titles across the top of each column containing titles like \ldblquote Written To\rdblquote  or \ldblquote Amount.\rdblquote   As you scroll through the spreadsheet, you want to keep the column headers visible, no matter how far down you go.  By putting the column headers in row 1 and splitting the screen right below the headers, you create two separately controlled areas.  You can scroll up or down in the bottom area, containing the various checks, with no effect on the top area, which has the headers.\par Another way to use split screens is to compare information in two widely separated parts of the spreadsheet.\par The split screen controls are the black rectangles to the left of the horizontal scroll bar and above the vertical scroll bar.  These controls are missing if the window is too small to split.\par To split the screen, grab the split screen control with the mouse and drag it to the location where you want the split to appear.  The screen will be split by a solid black line.  Assuming there is enough room, a scroll bar will appear on each side of the split screen control, and the split screen control itself will move to the location of the split.  Each scroll bar}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 scroll bars}}}{\fs20  works independently of the other, scrolling each area in any direction as far as you care to move.\par To get rid of a split screen, drag the split screen control to either end if the screen.\par }\pard\plain \s40\qj\widctlpar\adjustright \b\f4\fs36\cgrid {\fs28 \sect }\sectd \sbkodd\linex-32767\headery2520\footery2520\colsx144\endnhere\titlepg\sectdefaultcl {\headerr \pard\plain \s32\qr\widctlpar\adjustright \f4\cgrid {\f5\fs20 Chapter 4 \endash  Contents of a Cell\par }}\pard\plain \s40\qj\sa280\widctlpar\adjustright \b\f4\fs36\cgrid {\fs28 Chapter 4 \endash  Contents of a Cell}{\pard\plain \s40\qj\sa280\widctlpar\adjustright \b\v\f4\fs28\cgrid {\tc {\v\fs28 Chapter 4 \endash  Contents of a Cell\tcl1}}}{\fs28 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 This chapter is a technical reference to cells\emdash how to enter values, how to create functions, and a catalog of the various functions available in Quick Click Calc.\par The chapter is organized for easy reference, not as a tutorial.  For an introduction to the various capabilities of Quick Click Calc, see Chapter 2.  Refer to this chapter to find details about entering values and for a complete list of the spreadsheet\rquote s functions and how to use them.\par This chapter has three parts.\par \ldblquote Entering Spreadsheet Values and Labels\rdblquote  tells how to enter things in a spreadsheet cell.  It explains all of the ways to enter a numeric constant, how to enter text as a cell label, and how to create expressions that calculate values based on the contents of another cell.\par \ldblquote Functions by Category\rdblquote  lists all of the built in functions, like financial and mathematical operations.  In this section, the functions are listed by category, with a brief line describing what the function does.  This is a good section to use when you know what you want to do, but don\rquote t know the name of the Quick Click Calc function that will do what you want.\par \ldblquote Function Reference\rdblquote  is a complete list of all of the functions in Quick Click Calc in alphabetical order.  Each function is completely described, with examples.  In many cases, the examples show how to solve real-world problems with Quick Click Calc.\par }\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\f4\fs28\cgrid {\fs24 Entering Spreadsheet Values and Labels}{\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\v\f4\cgrid {\tc {\v\fs24 Entering Spreadsheet Values and Labels\tcl2}}}{\fs24 \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\f20\fs20 }{\b\fs20 \tab Note}{\fs20 \tab \ldblquote Using the Formula Entry Box\rdblquote  in Chapter 3 covers how to use the mouse and keyboard to enter information in a cell.  This section tells what to put in a spreadsheet cell.\~\~}{\f20\fs18 }{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par }{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 cells\:entering information}}}{\fs20 Any time a cell is selected, you can enter information in the cell by selecting the formula box and typing.  The information you enter can be a string, generally used to label information in a spreadsheet; a formula, which can calculate a value based on the contents of other cells; or a constant, which can take one of several forms.  The sections below cover the overall format for each of these types of information.\par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Cell Labels}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Cell Labels\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 cell labels}}}{\fs20 Cell labels are used to label information in a spreadsheet.  They are text entries, like the name of a student or a label identifying what is in a column of numbers.\par You can enter any text characters in a cell label.  In some cases, though, your label might be confused with a constant or formula\emdash especially since so many different kinds of constants are accepted!  The rules used by the spreadsheet to tell the difference between a cell label and the various forms of constants and expressions are fairly complicated.  There is one simple way to make sure what you type will be interpreted as a constant, though:  Use a leading \ldblquote  character.  As long as the first character you type is \ldblquote , everything after that character is treated as a cell label.  The \ldblquote  character will not be displayed.\par \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-3981\'73}{\b\fs20 \tab Important}{\fs20 \tab Since \ldblquote  has special meaning when it is used as the first character in a cell, you must use two \ldblquote  character to start a cell label with a \ldblquote  character.\~\~}{\outl\f21\fs14 \u-3981\'73}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par As for the complicated rules for telling the difference between a cell label and some other cell entry, they are:\par \par }\pard \s39\qj\fi-360\li720\widctlpar\adjustright {\fs20 \bullet \tab Anything that starts with a +, - or a numeric digit is a numeric constant.\par \bullet \tab Anything that fits the rules for a date, time or grade constant is a numeric constant.\par \bullet \tab Anything that looks like an expression is treated as an expression.\par \bullet \tab Anything that starts with an = character is treated as an expression.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-4016\'50}{\b\fs20 \tab Tip}{\fs20 \tab If you type something you intended to be a label, and a strange number shows up in the cell, it\rquote s because the string was interpreted as a constant.  For example, if you label some columns as A, B and C, you will see the numbers 4, 3 and 2, since these strings happen to be grade constants.  When this happens, select the cell and add a \ldblquote  character at the start of the string.\~\~}{\outl\f21\fs14 \u-4016\'50}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par Examples of cell labels are:\par }\pard \s39\qj\li720\widctlpar\adjustright {\fs20 Fred Smith\par \ldblquote January 1st, 1994\par Taxes Due:\par \ldblquote 7\par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Numeric Constants}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Numeric Constants\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 numbers}}}{\fs20 {\*\bkmkstart bk10}Numeric constants are cells with a single numeric value.  Numeric cells are used for individual numbers, like data points from an experiment or amounts from checks.\par The easy way to think of a numeric cell is anything you would normally write as a number.\par The technical rules are a but more involved.  Numeric constants can start with a + or - character.  This is followed by one or more numeric digits (\lquote 0\rquote  to \lquote 9\rquote ).  Next, you can encode a decimal point followed by one or more decimal digits.  Finally, you can encode an exponent, which is the letter e or E followed by an optional + or - character and one or more numeric digits.\par \par Examples of numeric constants are:\par }\pard \s39\qj\li720\widctlpar\tx2160\tx3600\tx5040\tx6480\adjustright {\fs20 300\tab 4.65\tab +7\tab -320000\tab 1e-10\par 4.567E6\tab 3.14159\tab 123E+100\tab 0\par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Date Constants}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Date Constants\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 dates}}}{\fs20 Date constants give you an easy way to enter dates and perform math on the result.  When you enter a date constant, the date is converted to a number.  If you add a value to the date, the result is the numeric form of a different date.\par For example, let\rquote s say you enter the date\par \par July 4th, 1776\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cell C4.  In another cell, you enter the formula\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par =C4+1\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 The result is the numeric value for July 5th, 1776.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-3981\'73}{\b\fs20 \tab Important}{\fs20 \tab A date is a special form of a numeric constant, and it\rquote s displayed that way.  When you first enter a date, it shows up as a number.  Use the Cell Attributes... command to change the format to one of the date formats.\~\~}{\outl\f21\fs14 \u-3981\'73}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par To enter a date, type the date with no leading characters.  Quick Click Calc supports the following formats for dates:\par \par }\pard \s39\qj\li360\ri1620\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \tx2160\tx4320\adjustright {\fs20 template\tab examples\tab date\par }\pard \s39\qj\li360\ri1440\keepn\widctlpar\tx2160\tx4320\adjustright {\fs20 mm/dd/yy\tab 6/27/55\tab June 27th, 1955\par \tab 12/25/94\tab December 25th, 1994\par \tab 7/4/1776\tab July 4th, 1776\par day month year\tab 27 Jun 55\tab June 27th, 1955\par day month, year\tab 25 Decem, 94\tab December 25th, 1994\par \tab 4 July, 1776\tab July 4th, 1776\par month day year\tab Jun 27, 1955\tab June 27th, 1955\par month day, year\tab Decemb 25, 94\tab December 25th, 1994\par }\pard \s39\qj\li360\ri1440\widctlpar\tx2160\tx4320\adjustright {\fs20 \tab July 4, 1776\tab July 4th, 1776\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par For months, you must enter at least the first three characters of the name of the month.  You may enter as many other characters as you like, but all characters entered must be correct.  The case of the letters does not matter.\par In all cases, when a year is less than 100, 1900 is added.\par \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\f20\fs20 }{\b\fs20 \tab Note}{\fs20 \tab }{\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 dates}}}{\fs20 The internal numbers used by Quick Click Calc give accurate dates for the entire range of the modern Gregorian calendar}{\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 Gregorian calendar}}}{\fs20 , used in most of the world today.  History buffs are aware that the Julian calendar}{\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 Julian calendar}}}{\fs20 , which the Gregorian calendar}{\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 calendar}}}{\fs20  replaced, wasn\rquote t universally accepted until fairly recently.  If you are using the spreadsheet for historical calculations\emdash for example, computing the number of days between historical dates\emdash be sure all dates are expressed using the Gregorian calendar.\~\~}{\f20\fs18 }{\b\fs20 \par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Time Constants}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Time Constants\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 times}}}{\fs20 Time constants give you an easy way to enter times and perform math on the result.  When you enter a time constant, the time is converted to an internal number.  If you add a value to the time, the result is the numeric form of a different time.\par For example, let\rquote s say you enter the time\par \par 6:30:00\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cell D4.  In another cell, you enter the formula\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par =D4+45\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 The result is the numeric value for 7:15.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-3981\'73}{\b\fs20 \tab Important}{\fs20 \tab A time is a special form of a numeric constant, and it\rquote s displayed that way.  When you first enter a time, it shows up as a number.  Use the Cell Attributes... command to change the format to Time.\~\~}{\outl\f21\fs14 \u-3981\'73}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par A time is one or more numeric digits (the hour), a colon, one or more numeric digits (minutes), a second colon, and a third set of digits (seconds):\par \par hh:mm:ss\par \par Times are converted to numbers by multiplying the minutes by 60, the hours by 3600, and adding these two numbers to the seconds field.\par \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-4016\'50}{\b\fs20 \tab Tip}{\fs20 \tab Quick Click Calc isn\rquote t picky about the actual values in the time fields.  If you want to enter a time as 90 seconds, 0:0:90 will work just fine.  When you display the time, the value shows up as 00:01:30.\~\~}{\outl\f21\fs14 \u-4016\'50}{\b\fs20 \par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Grade Constants}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Grade Constants\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 grades}}}{\fs20 Grade constants give you an easy way to enter student grades and perform math on the result.  When you enter a grade constant, the grade is converted to a number based on a 4 point grading system.\par To enter a grade, enter the letter, optionally followed by a + or -.  The case of the letter is not important; B+ and b+ both work just fine.\par For example, let\rquote s say you enter the grades\par \par }\pard \s39\qj\li360\widctlpar\tx1080\tx1800\tx2520\adjustright {\fs20 A-\tab B\tab B+\tab C\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cells E4 through H4.  In another cell, you enter the formula\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par =Avg(E4:H4)\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 The result is the numeric value for B.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-3981\'73}{\b\fs20 \tab Important}{\fs20 \tab A grade is a special form of a numeric constant, and it\rquote s displayed that way.  When you first enter a grade, it shows up as a number.  Use the Cell Attributes... command to change the format to Grade.\~\~}{\outl\f21\fs14 \u-3981\'73}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par Grades are converted to numbers based on the table below.  When converting from a number back to a letter grade, the numeric value is rounded to the closest letter grade.  A grade of F+ or F- is displayed as F.\par \par }\pard \s39\qj\li360\ri5760\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \tx1440\adjustright {\fs20 Grade\tab Value\par }\pard \s39\qj\li360\ri5760\widctlpar\tx1440\adjustright {\fs20 A+\tab 4 }{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3}{\fs20 \par A\tab 4\par A-\tab 3 }{\fs18\up4 2}{\fs20 /}{\fs18\dn4 3}{\fs20 \par B+\tab 3 }{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3}{\fs20 \par B\tab 3\par B-\tab 2 }{\fs18\up4 2}{\fs20 /}{\fs18\dn4 3}{\fs20 \par C+\tab 2 }{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3}{\fs20 \par C\tab 2\par C-\tab 1 }{\fs18\up4 2}{\fs20 /}{\fs18\dn4 3}{\fs20 \par D+\tab 1 }{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3}{\fs20 \par D\tab 1\par D-\tab }{\fs18\up4 2}{\fs20 /}{\fs18\dn4 3}{\fs20 \par F+\tab }{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3}{\fs20 \par F\tab 0\par F-\tab -}{\fs18\up4 1}{\fs20 /}{\fs18\dn4 3\par {\*\bkmkend bk10}}{\pard\plain \s39\qj\li360\ri5760\widctlpar\tx1440\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 constants}{\rxe bk10}}}{\v\fs20 \par }\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 Formulas}{\pard\plain \s38\qj\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\tc {\v\fs20 Formulas\tcl3}}}{\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 An formula cell}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 formula cell}}}{\fs20  is a computed value that generally involves more than one numeric value, some of which are values from other cells.  Any cell can be used as a term in a formula.\par There are two distinct kinds of values that can appear in expressions, and can be the result of an expression.  The most common is a numeric value, like 4.5.  The other kind of value is a string.  In general, expressions involving strings and expressions involving numbers are quite different, and they will be discussed separately here.  There are functions to convert from numbers to strings and strings to numbers, though, and using these functions, you can mix strings and numbers in the same expression.\par \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 String Formulas}{\pard\plain \s39\qj\keepn\widctlpar\adjustright \b\v\f4\fs20\cgrid {\tc {\b\v\fs20 String Formulas\tcl4}}}{\b\fs20 \par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 string formulas}}}{\fs20 String formulas use strings}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 strings}}}{\fs20  for arguments and string functions to operate on the strings.  The result is a string, which looks and works like a label cell.  For example, you might use a string formula to build a complicated label, like this:\par \par =Concat(\ldblquote Grades for \ldblquote , B5)\par \par There are no operations for strings, like the + or * operators for numbers.  Functions are always used to manipulate strings.  You can find detailed descriptions of the string functions, along with examples showing how to use them, in \ldblquote Function Reference,\rdblquote  later in this chapter.\par \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Numeric Formulas}{\pard\plain \s39\qj\keepn\widctlpar\adjustright \b\v\f4\fs20\cgrid {\tc {\b\v\fs20 Numeric Formulas\tcl4}}}{\b\fs20 \par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 number formulas}}}{\fs20 Numeric formulas involve one or more numbers and the standard math operations, as well as mathematical functions.  The various operators appear either before a number, as in the - sign, or between numbers.  One operator, the % operator, even appears after the number it operates on.  The expression is evaluated left to right.\par For example, the formula\par \par 1 + Abs(-2)*3\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 returns 9.  Working from left to right, 1 + Abs(-2) is 3, and 3*3 is 9.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par There are dozens of functions available, and they perform a wide variety of operations.  The functions are covered in \ldblquote Function Reference,\rdblquote  later in this chapter.\par The various operators you can use in a function are shown below.\par \par }\pard \s39\qj\fi-1080\li1440\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \tx1800\adjustright {\fs20 operation\tab meaning\par }\pard \s39\qj\fi-1080\li1440\widctlpar\tx1800\adjustright {\fs20 +\tab When used before a number, this operator does nothing.  It indicates that a number is positive, but then, numbers are positive by default.\par \tab \tab When used between numbers, + adds the two numbers.\par -\tab When used before a number, this operator changes the sign of the number.\par \tab \tab When used between numbers, - subtracts the number to the right from the number to the left.\par *\tab Multiplies the number to the left and the number to the right.\par /\tab Divides the dumber to the left (numerator) by the number to the right (denominator).\par ^\tab Raises the number to the left to the power of the number to the right.  For example, 2^3 is the same as 2*2*2, which is 8.\par <>\tab Returns 1 (true) if the number to the left is not equal to the number to the right, and 0 (false) otherwise.\par =\tab Returns 1 (true) if the number to the left is equal to the number to the right, and 0 (false) otherwise.\par <\tab Returns 1 (true) if the number to the left is less than the number to the right, and 0 (false) otherwise.\par <=\tab Returns 1 (true) if the number to the left is less than or equal to the number to the right, and 0 (false) otherwise.\par >\tab Returns 1 (true) if the number to the left is greater than the number to the right, and 0 (false) otherwise.\par >=\tab Returns 1 (true) if the number to the left is greater than or equal to the number to the right, and 0 (false) otherwise.\par $\tab The dollar operator doesn\rquote t really do anything, but it helps you remember what is happening in a formula.  You can use $ before any number, as in $3.25.\par %\tab This operator appears }{\i\fs20 after}{\fs20  a number.  It converts a number to a percentage by dividing the number by 100.  For example 17% is the same as 0.17.\par ( )\tab Parenthesis can be used in pairs to group calculations, changing the order in which they are performed.  For example, 1+2*3 is 9, but 1+(2*3) is 7.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Boolean Values}{\pard\plain \s39\qj\keepn\widctlpar\adjustright \b\v\f4\fs20\cgrid {\tc {\b\v\fs20 Boolean Values\tcl4}}}{\b\fs20 \par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 boolean}}}{\fs20 In many situations a value is either true or false, rather than a number.  For example, when you use the IF function to select an alternative, there is a condition that is either true or false.  In most cases, you\rquote ll use a boolean function or one of the operators that returns a boolean result.  For example, if you want to prevent a number from being negative, you might use the expression\par \par if (D4 < 0, 0, D4)\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 This expression tests the value in cell D4 to see if it is less than zero.  If so, the function returns the second value, 0.  If D4 is not less than zero, the function returns the value in D4.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 While you will usually distinguish between a boolean value of true or false and a number by the way you use the values in an expression, Quick Click Calc doesn\rquote t need two different kinds of value to keep boolean values and numbers straight.  As far as the program is concerned, any number can be used as a boolean value, and vice versa.  Any function or operation that returns true}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 true}}}{\fs20  returns the number 1, and any boolean function or operation that returns false}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 false}}}{\fs20  returns the number 0.  When you use a number as a boolean value, 0 is treated as false, and any other number is treated as true.\par \par }\pard\plain \qj\fi-1800\li1800\ri1080\widctlpar\tx440\adjustright \f4\cgrid {\outl\f21\fs20 \u-4016\'50}{\b\fs20 \tab Tip}{\fs20 \tab The way boolean values and numbers are used interchangeably is sometimes very useful.  For example, if you have a series of boolean results in cells D1 to D100\emdash test results, perhaps\emdash you can count the number of true values very quickly with the expression Sum(D1:D100).\~\~}{\outl\f21\fs14 \u-4016\'50}{\b\fs20 \par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Cell Ranges and Lists}{\pard\plain \s39\qj\keepn\widctlpar\adjustright \b\v\f4\fs20\cgrid {\tc {\b\v\fs20 Cell Ranges and Lists\tcl4}}}{\b\fs20 \par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 number formulas}}}{\fs20 Many functions let you enter a list.  The list might be a string list, like CONCAT, or it might be a list of numbers, as in SUM.  The list is a very flexible way of entering numbers.  Unless the description of the function says otherwise, all of the methods described in this section work with each function that accepts a list.\par The list is made up of one or more entries separated by commas.  Each entry can be a number (or string, for a string function), the name of a cell, or a range of cells.\par A range of cells is a cell name, a colon, and another cell name.  The two cells form the corners of a rectangle of cells, and all of the cells in the rectangle are used.  Here are a few examples of cell ranges and the cells they include:\par \par }\pard \s39\qj\li360\ri3240\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \adjustright {\fs20 range\tab cells in the range\par }\pard \s39\qj\li360\keepn\widctlpar\adjustright {\fs20 D4:D7\tab D4, D5, D6, D7\par C2:F2\tab C2, D2, E2, F2\par }\pard \s39\qj\li360\widctlpar\adjustright {\fs20 A1:C3\tab A1, A2, A3, B1, B2, B3, C1, C2, C3\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par All of the values in the list are used by the functions that work on lists.  For example, SUM adds all of the numbers in a list, so\par \par Sum(A1, 3, C2:C4)\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 adds 3 and the cell values in cells A1, C2, C3 and C4.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 In most cases, if a function can\rquote t use a value in a cell, it uses a reasonable substitute.  SUM uses the value 0 for any cell that doesn\rquote t have a value; PRODUCT, which multiplies cell values, uses 1.  In both cases, the way an unusable cell is treated has the smallest possible impact on the cells that can be used.  If you need to know the exact rules, see the documentation for the function, but in general, things just work the way you would expect\emdash and there\rquote s no real need to get more technical than that!\par \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Fixed Cells}{\pard\plain \s39\qj\keepn\widctlpar\adjustright \b\v\f4\fs20\cgrid {\tc {\b\v\fs20 Fixed Cells\tcl4}}}{\b\fs20 \par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\fi360\widctlpar\adjustright {\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 cells\:fixed}}}{\fs20 You can use the $ character before the column or row name of a cell to force the cell reference to remain fixed as the spreadsheet is updated.\par As you move formula cells around using Copy and Paste, insert and delete columns and rows, fill ranges of cells with the Fill Down command, and do all of the other editing that comes natural as you create and manipulate spreadsheets, the cells formulas refer to change.  As this happens, Quick Click Calc does its best to update the cell references by adjusting the cell names in formulas.\par For example, if a formula refers to cell C3 and you delete column B, the information that was in cell C3 ends up in cell B3.  The formula that contained the reference to cell C3 is automatically updated by changing the cell name from C3 to B3.\par There are some situations where you don\rquote t want this to happen.  For example, a calculation showing principal on a loan by month might be arranged as a series of formula cells, each of which calculates the new principal based on the principal from the month before, appearing in the cell above, and an interest rate, which appears in a fixed cell.  It\rquote s natural to create the column of formulas using the Fill Down command.  Updating the cell reference to the cell above is done automatically, but the reference to the interest rate is also updated as the formula is moved down, and that\rquote s not what is needed.\par Let\rquote s assume that the interest rate is in cell C1, and the initial principal is in cell B1.  The formula\par \par = B1*C1 - $800\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cell B2 calculates the new principal after accruing interest for one month and making a payment of $800.  Selecting cells B2 and B3 and filling down gives this formula in cell B3:\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par = B2*C2 - $800\par \par That\rquote s almost what we want.  Cell B2 contains the most recent principal, which is what we want to use in this formula from cell B3.  The interest rate is in cell C1, though, not cell C2.\par What we need is a way to keep the reference to cell C1 fixed as the formula is adjusted by the Fill Down command.  Placing a $ character before the 1 fixes the cell row.  Using the formula:\par \par = B1*C$1 - $800\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cell B2 and filling down gives:\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 \par = B2*C$1 - $800\par \par }\pard \s39\qj\widctlpar\adjustright {\fs20 in cell B3.\par }\pard \s39\qj\fi360\widctlpar\adjustright {\fs20 You can also use a $ before the column name of a cell, or even use the $ character before both the column and row.\par }\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\f4\fs28\cgrid {\fs24 Functions by Category}{\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\v\f4\cgrid {\tc {\v\fs24 Functions by Category\tcl2}}}{\fs24 \par }\pard\plain \s39\keepn\widctlpar\adjustright \f4\cgrid {\b\fs20 Mathematics\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 ABS\tab Absolute value\par ACOS\tab Arc cosine\par ASIN\tab Arc sine\par ARCTAN2\tab Two-argument arc tangent\par ATAN\tab Arc tangent\par AVG\tab Return the average of a list of numbers\par COS\tab Cosine\par DEG\tab Convert radians to degrees\par EXP\tab Exponent\par FRAC\tab Returns the fractional part of a number\par INT\tab Returns the whole part of a number\par LN\tab Natural logarithm\par LOG\tab Base 10 logarithm\par MOD\tab Remainder\par PRODUCT\tab Returns the product of a list of numbers\par RAD\tab Convert degrees to radians\par RANDOM\tab Random number\par ROUND\tab Round to the closest number\par SGN\tab Sign of a number\par SIN\tab Sine\par SQRT\tab Square root\par STDV\tab Standard deviation\par SUM\tab Add a list of numbers\par TAN\tab Tangent\par VAR\tab Variance\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Financial\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 FV\tab Future value\par IRR\tab Internal rate of return\par NPV\tab Net present value\par PMT\tab Periodic payment\par PV\tab Present value\par RATE\tab Interest rate\par TERM\tab Number of periods for a periodic payment\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Miscellaneous Functions\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 ALERT\tab Present an alert\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Strings\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 ASC\tab Return the ASCII value for a character\par CHR\tab Convert a number to a character\par CONCAT\tab Concatenate strings\par LEN\tab Returns the number of characters in a string\par LOWER\tab Convert a string to lowercase\par POSITION\tab String search\par PROPER\tab Converts a string to lowercase with a leading uppercase letter\par STRING\tab Convert a number to a string\par SUBSTR\tab Return a portion of a string\par UPPER\tab Convert a string to uppercase\par VAL\tab Convert a string to a number\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Dates\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 DATE\tab Returns the date string for a number\par DAY\tab Return the day of month from a date string\par MONTH\tab Returns the month from a date string\par NUMDATE\tab Returns the numeric equivalent for a date\par TODAY\tab Today\rquote s date\par WEEKDAY\tab Return the weekday for a date string\par YEAR\tab Return the year from a date string\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Boolean Functions\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 AND\tab Logical and of a list of values\par IF\tab Return one of two values based on a condition\par ISBLANK\tab Checks to see if a string is empty\par ISEMPTY\tab Checks to see if a cell is empty\par ISERROR\tab Checks to see if a cell returns *Error*\par ISNA\tab Checks to see if a cell returns N/A\par ISNUMBER\tab Checks to see if a cell contains a number\par ISSTRING\tab Checks to see if a cell contains a string\par NOT\tab Boolean negation\par OR\tab Logical or of a list of values\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Picking, Counting & Selecting Cells & Values\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 CHOOSE\tab Pick one item from a series\par COUNT\tab Counts the number of values in a range of cells\par HLOOKUP\tab Return a value from a horizontal table\par MAX\tab Returns the largest number in a list\par MIN\tab Returns the smallest number in a list\par VLOOKUP\tab Return a value from a vertical table\par \par }\pard \s39\keepn\widctlpar\adjustright {\b\fs20 Constants\par }\pard \s39\fi360\widctlpar\tx1800\adjustright {\fs20 ERROR\tab *Error* constant\par FALSE\tab Boolean constant for false\par INF\tab Infinity\par NA\tab N/A constant\par NULL\tab Null string constant\par PI\tab Numeric constant; approximately 3.14159\par TRUE\tab Boolean constant for true\par }\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\f4\fs28\cgrid {\fs24 Function Reference}{\pard\plain \s37\qj\sb560\sa280\keepn\widctlpar\brdrt\brdrs\brdrw30 \adjustright \b\v\f4\cgrid {\tc {\v\fs24 Function Reference\tcl2}}}{\fs24 \par }\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 ABS}{\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 ABS}}}{\fs20  ( number )\par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 Returns the absolute value}{\pard\plain \s39\qj\fi360\widctlpar\adjustright \v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 absolute value}}}{\fs20  of the number.\par \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Examples\par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\li360\ri3420\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \tx3600\adjustright {\fs20 Expression\tab Returns\par }\pard \s39\qj\fi360\widctlpar\tx3600\adjustright {\fs20 Abs(-4)\tab 4\par Abs(7.3)\tab 7.3\par }\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 ACOS}{\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 ACOS}}}{\fs20  ( number )\par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 Returns the arc cosine of the number.\par The number must be in the range -1 to 1.  If it is not, ACOS returns *Error*.\par The result is an angle expressed in radians.  It will be in the range 0 to \u960\'b9.\par \par }\pard \s39\qj\keepn\widctlpar\adjustright {\b\fs20 Examples\par }\pard \s39\qj\fi360\keepn\widctlpar\adjustright {\fs20 \par }\pard \s39\qj\li360\ri3420\keepn\widctlpar\brdrt\brdrs\brdrw15 \brdrb\brdrs\brdrw15 \tx3600\adjustright {\fs20 Expression\tab Returns\par }\pard \s39\qj\fi360\widctlpar\tx3600\adjustright {\fs20 ACos(5/4)\tab 0.64350\par }\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\f4\cgrid {\fs20 ALERT}{\pard\plain \s41\sb480\sa240\keepn\widctlpar\brdrt\brdrs\brdrw15 \adjustright \b\v\f4\fs20\cgrid {\xe {\v\fs20 }{\v\fs20 ALERT}}}{\fs20  ( string )\par }\pard\plain \s39\qj\fi360\widctlpar\adjustright \f4\cgrid {\fs20 Displays an alert with }{\i\fs20 string}{\fs20  as the text of the alert.  ALERT always returns the number 0 as a result.\par ALERT is usually used to flag an error condition.  For example, if you want to be warned if a student is failing, you could use an expression like:\par \par }{\f6\fs18 if(C6 = 0, Alert(Concat(A6, \ldblquote  is failing!  Call the parent!\rdblquote )), 0)\par }{\fs20 \par In this example, the cell A6 would contain the name of the student, and the cell C6 would contain the grade.  The numeric value of an F is 0, so the expression checks to see if the cell C6 is zero.  If so, the student is failing, and the first expression is evaluated.  The first expression is an alert.  If C6 is not zero, the student is not failing, and the second condition is evaluated.  That one just returns zero.\par The alert itself uses the CONCAT function to build a string.  Cell A6 contains the students name; this is combined with the string constant to create a custom string for the alert.  If C6 is 0, and A6 contains the label Johnny, you would see this alert:\par \par }\pard \s39\qc\widctlpar\adjustright {{\pict{\*\picprop\shplid1082{\sp{\sn shapeType}{\sv 75}}{\sp{\sn fFlipH}{\sv 0}}{\sp{\sn fFlipV}{\sv 0}}{\sp{\sn fLine}{\sv 0}}}\picscalex33\picscaley33\piccropl0\piccropr0\piccropt0\piccropb0\picw324\pich144\picwgoal6480\pichgoal2880\macpict\bliptag-1314505796\blipupi72{\*\blipuid b1a63bbcbff8ac7eded1af5f1af83ffa}09cf00000000009001441101a00082a0008e01000a000000000308028098002a000000000090014800000000009001440000000000900144000105d9ff01f00005d9ff01f0000700c0da000130000700c0da000130000700c0da000130000700c0da000130000800c1dbff02f830000800c1dbff02f830000800c1dbff02f830000800c1dbff02f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830000901c1f0dc0002f830002602c1f00ffcff04f000060060fe0010030000700187060000180078007070000cfb0002f830002602c1f00ffcff04f000060060fe0010030000700187060000180078007070000cfb0002f830002504c1f00c003ffeff04f000060060fb0002c00003fe00071800cc003030060cfb0002f830002504c1f00c003ffeff04f000060060fb0002c00003fe00071800cc003030060cfb0002f830002a04c1f00c003ffeff03f000063cfe7c1366070f01f1f3830e3e1f1800c07c30300f8f8780fd0002f830002a04c1f00c003ffeff03f000063cfe7c1366070f01f1f3830e3e1f1800c07c30300f8f8780fd0002f83000280ac1f00c0c3ff0003ff00006fc6612031800c331830633331800c0cc3030060cccc0fd0002f83000280ac1f00c0c3ff0003ff00006fc6612031800c331830633331800c0cc3030060cccc0fd0002f83000290ac1f00c0c3fc0000ff00006fd66136e030f00c331830633331800c0cc3030060ccfc0fd0002f83000290ac1f00c0c3fc0000ff00006fd66136e030f00c331830633331800c0cc3030060ccfc0fd0002f830002709c1f00c003f000003f000fc661236030180c3318306331f0000cccc303006ccccfc0002f830002709c1f00c003f000003f000fc661236030180c3318306331f0000cccc303006ccccfc0002f830002a0bc1f00c003f0f33f3f0003c3cfe661346078f00c1f3c78f33231800787c7878038cc780fd0002