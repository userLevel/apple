r \par \hich\af6\dbch\af4\loch\f6    tcpDGMSUDP: lon\hich\af6\dbch\af4\loch\f6 gint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSUDPBAD: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSUDPNOPORT: longint;\par \par \hich\af6\dbch\af4\loch\f6    tcpDGMSTCP: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSTCPBAD: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSTCPNOPORT: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSTCPQUEUED: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSTCPOLD: longint;\par \par \hich\af6\dbch\af4\loch\f6    tcpDGMSOFRAGMENTS: longint;\par \hich\af6\dbch\af4\loch\f6    tcpDGMSFRAGMENTED: \hich\af6\dbch\af4\loch\f6 longint;\par \hich\af6\dbch\af4\loch\f6    end;\tab \tab \tab \par \hich\af6\dbch\af4\loch\f6 errTablePtr = ^errTable;\par }\pard\plain \s18\widctlpar\adjustright \f4\cgrid {\par }\pard\plain \s25\fi-2260\li2260\widctlpar\adjustright \f4\cgrid {\f6 errTablePtr}{\tab The value returned is a pointer to the error table. The error table is read only, and is provided for reference only.\par }\pard\plain \s23\fi-4240\li4240\widctlpar\tx3220\adjustright \f4\cgrid {\par }\pard \s23\fi-4240\li4240\keepn\widctlpar\tx3220\adjustright {The currently defined error table offsets are:\par \par }{\f6 tcpDGMSTBLEN}{\tab +0000\tab The total length of the error table, in bytes, including }{\f6 tcpDGMSTBLEN}{\par }{\f6 tcpDGMSTOTAL}{\tab +0004\tab Total datagrams received (good and bad)\par }{\f6 tcpDGMSFRAGSIN}{\tab +0008\tab Got a fragment (datagram is queued to frag list)\par }{\f6 tcpDGMSFRAGSLOST}{\tab +0012\tab Fragment purged after timeout in queue\par }\pard \s23\fi-4240\li4240\widctlpar\tx3220\adjustright {\f6 tcpDGMSBUILT}{\tab +0016\tab Built a datagram from fragments (is then queued)\par \par }{\f6 tcpDGMSOK}{\tab +0020\tab Datagrams queued from link or }{\f6 tcpDGMSBUILT}{\par \par }{\f6 tcpDGMSBADCHK}{\tab +0024\tab Bad IP checksum (datagram is purged)\par }{\f6 tcpDGMSBADHEADLEN}{\tab +0028\tab Bad IP header lengths (datagram is purged)\par }{\f6 tcpDGMSBADPROTO}{\tab +0032\tab Unsupported protocols (added to misc queue)\par }{\f6 tcpDGMSBADIP}{\tab +0036\tab Not my or loopback IP (datagram is purged)\par \par }{\f6 tcpDGMSICMP}{\tab +0040\tab ICMP total datagrams in (good and bad)\par }{\f6 tcpDGMSICMPUSER}{\tab +0044\tab ICMP user datagrams\par }{\f6 tcpDGMSICMPKERNEL}{\tab +0048\tab ICMP kernel datagrams\par }{\f6 tcpDGMSICMPBAD}{\tab +0052\tab ICMP bad checksum or datagram too short\par }{\f6 tcpDGMSICMPBADTYPE}{\tab +0056\tab ICMP bad ic_type\par }{\f6 tcpDGMSICMPBADCODE}{\tab +0060\tab ICMP bad ic_code\par }{\f6 tcpDGMSICMPECHORQ}{\tab +0064\tab ICMP ECHORQs in\par }{\f6 tcpDGMSICMPECHORQOUT}{\tab +0068\tab ICMP ECHORQ replies sent out\par }{\f6 tcpDGMSICMPECHORP}{\tab +0072\tab ICMP ECHORPs in\par }{\f6 tcpDGMSICMPECHORPBADID}{\tab +0076\tab ICMP ECHORPs unclaimed\par \par }{\f6 tcpDGMSUDP}{\tab +0080\tab UDPs OK (added to UDP queue)\par }{\f6 tcpDGMSUDPBAD}{\tab +0084\tab Bad UDP header (datagram is purged)\par }{\f6 tcpDGMSUDPNOPORT}{\tab +0088\tab No such logged in port (datagram is purged)\par \par }{\f6 tcpDGMSTCP}{\tab +0092\tab TCPs OK (returned to TCP main logic)\par }{\f6 tcpDGMSTCPBAD}{\tab +0096\tab Bad TCP header or checksum (datagram is purged)\par }{\f6 tcpDGMSTCPNOPORT}{\tab +0100\tab No such logged in port (datagram is purged)\par }{\f6 tcpDGMSTCPQUEUED}{\tab +0104\tab Arrived before required (datagram is queued)\par }{\f6 tcpDGMSTCPOLD}{\tab +0108\tab Already received this segment (datagram is purged)\par }\pard\plain \widctlpar\adjustright \f4\cgrid {\par }\pard\plain \s23\fi-4240\li4240\widctlpar\tx3220\adjustright \f4\cgrid {\f6 tcpDGMSOFRAGMENTS}{\tab +0112\tab Fragments transmitted\par }{\f6 tcpDGMSFRAGMENTED}{\tab +0116\tab Datagrams fragmented for transmission\par }\pard\plain \s21\widctlpar\brdrt\brdrs\brdrw15\brsp20 \tx4500\adjustright \b\f4\fs28\cgrid {\page {\*\bkmkstart _Toc422750602}TCPIPGetReconnectStat