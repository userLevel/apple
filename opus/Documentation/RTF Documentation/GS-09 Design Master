terminator            \}\par end;\par \par menu02itemtitle00  :=  'New';\par with menu02item00 do begin\par   version       :=  0;\par   itemID        :=  257;                                   \{new ID             \}\par   itemChar      :=  $4E;                                   \{shortcut characters\}\par   itemAltChar   :=  $6E;\par   itemCheck     :=  $0000;\par   itemFlag      :=  $0000;                          \{will pass pointer to title\}\par   itemTitleRef  :=  ord4 (@menu02itemtitle00);      \{pointer to item's name    \}\par end;\par \par menu02itemtitle01  :=  'Open...';\par with menu02item01 do begin\par   version       :=  0;\par   itemID        :=  258;                                   \{open ID            \}\par   itemChar      :=  $4F;                                   \{shortcut characters\}\par   itemAltChar   :=  $6F;\par   itemCheck     :=  $0000;\par   itemFlag      :=  $0000;                          \{will pass pointer to title\}\par   itemTitleRef  :=  ord4 (@menu02itemtitle01);      \{pointer to item's name    \}\par end;\par \par menu02itemtitle02  :=  'Close';\par with menu02item02 do begin\par   version       :=  0;\par   itemID        :=  255;                                   \{close ID           \}\par   itemChar      :=  $57;                                   \{shortcut characters\}\par   itemAltChar   :=  $77;\par   itemCheck     :=  $0000;\par   itemFlag      :=  $0000;                          \{will pass pointer to title\}\par   itemTitleRef  :=  ord4 (@menu02itemtitle02);      \{pointer to item's name    \}\par end;\par \par }\pard \keepn\widctlpar\adjustright {\f6\fs14 menu02itemtitle03  :=  'Save';\par with menu02item03 do begin\par   version       :=  0;\par   itemID        :=  259;                                   \{save ID            \}\par   itemChar      :=  $53;                                   \{shortcut characters\}\par }\pard \widctlpar\adjustright {\f6\fs14   itemAltChar  