 Click }{\f6\fs18 Cancel}{\f4\fs20  to exit the editor without creating a new icon.\par After creating an icon, you are presented with another dialog that requests further definition of the icon:\par \par }\pard \qc\fi540\widctlpar\adjustright {\fs20 {\pict{\*\picprop\shplid1133{\sp{\sn shapeType}{\sv 75}}{\sp{\sn fFlipH}{\sv 0}}{\sp{\sn fFlipV}{\sv 0}}{\sp{\sn fillColor}{\sv 268435473}}{\sp{\sn fFilled}{\sv 0}}{\sp{\sn fLine}{\sv 0}}}\picscalex50\picscaley50\piccropl0\piccropr0\piccropt0\piccropb0\picw303\pich368\picwgoal6060\pichgoal7360\macpict\bliptag-1055901522\blipupi72{\*\blipuid c11038ae445e1e915e069939999aee32}1c09000000000170012f1101a00082a0008e01000a0000000002d002409800260000000000510130000000000051012f000000000051012f000102db0006007fddff00fe06007fddff00fe060060dd000006060060dd000006060060dd000006060060dd000006060060ddff0006060060ddff0006060060ddff0006060060ddff0006080160f8df00011f06080160f8df00011f061b0160f8fe000501e030318006fe00021c01e0fe00003ef200011f061b0160f8fe000501e030318006fe00021c01e0fe00003ef200011f061e0160f8fe000003fe300006fd00020c00c0fe000033fc000018f800011f061e0160f8fe000003fe300006fd00020c00c0fe000033fc000018f800011f06240160f8fe00180331f1f38f8e1e3e1f0c00c1e1e3e0331f363efe1e3e1e363cfb00011f06240160f8fe00180331f1f38f8e1e3e1f0c00c1e1e3e0331f363efe1e3e1e363cfb00011f06240160f8fe0005033333318606fe330f0c00c33333303e333866db3318333860fb00011f06240160f8fe0005033333318606fe330f0c00c33333303e333866db3318333860fb00011f06240160f8fe000503f333318606fe330f0c00c303333030333066db3f183f303cfb00011f06240160f8fe000503f333318606fe330f0c00c303333030333066db3f183f303cfb00011f06240160f8fe00050333333186c6fe330f0c00c333333030333066db301b303006fb00011f06240160f8fe00050333333186c6fe330f0c00c333333030333066db301b303006fb00011f06240160f8fe00180331f1f3c38f1e331f1e01e1e1e330301f303edb1e0e1e303cfb00011f06240160f8fe00180331f1f3c38f1e331f1e01e1e1e330301f303edb1e0e1e303cfb00011f06080160f8df00011f06080160f8df00011f06080160f8df00011f06080160f8df00011f06080160f8df00