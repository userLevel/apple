lpar\adjustright {\f6\fs20 BUTLAST object\par BL object\par }\pard \fi360\widctlpar\adjustright {\f6\fs20 CHAR number\par }\pard \widctlpar\adjustright {\f6\fs20 COUNT object\par EMPTYP object\par EQUALP object1 object2\par FIRST object\par FLOATP object\par FPUT object list\par INTEGERP object\par ITEM integer object\par LAST object\par LIST object1 object2\par }\pard \fi-180\li540\widctlpar\adjustright {\f6\fs20 (LIST object1 object2 object3 ... )\par }\pard \widctlpar\adjustright {\f6\fs20 LISTP object\par LOWERCASE word\par LPUT object list\par MEMBER object1 object2\par MEMBERP object1 object2\par NUMBERP object\par PARSE word\par SENTENCE object1 object2\par }\pard \fi360\widctlpar\adjustright {\f6\fs20 SE object1 object2\par }\pard \fi-180\li540\widctlpar\adjustright {\f6\fs20 (SENTENCE object1 object2 object3 ... )\par (SE object1 object2 object3 ... )\par }\pard \widctlpar\adjustright {\f6\fs20 UPPERCASE word\par WORD word1 word2\par }\pard \fi360\widctlpar\adjustright {\f6\fs20 (WORD word1 word2 word3 ... )\par }\pard \widctlpar\adjustright {\f6\fs20 WORDP object\par \par }\pard\plain \s38\keepn\widctlpar\adjustright \b\f4\fs28\cgrid {\fs20 Property Lists\par \par }\pard\plain \widctlpar\adjustright \f4\cgrid {\f6\fs20 GPROP name property\par PLIST name\par PPROP name property value\par REMPROP name property\par SETPLIST name list\par \par }\pard\plain \s38\keepn\widctlpar\adjustright \b\f4\fs28\cgrid {\fs20 Numbers and Arithmetic\par \par }\pard\plain \widctlpar\adjustright \f4\cgrid {\f6\fs20 ABS value\par ARCCOS value\par ARCSIN value\par ARCTAN value\par ARCTAN2 x y\par COS angle\par DIFFERENCE value1 value2\par EXP number\par FLOAT number\par FORM number width digits\par INT number\par }\pard \fi-180\li180\widctlpar\adjustright {\f6\fs20 INTQUOTIENT numerator denominator\par }\pard \widctlpar\adjustright {\f6\fs20 LN number\par POWER x y\par PRODUCT number1 number2\par }\pard \fi-180\li540\widctlp