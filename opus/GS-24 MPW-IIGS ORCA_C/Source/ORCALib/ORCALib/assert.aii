	case  on	seg   ''******************************************************************  Assert - Condition assertion macro**  This code implements the subroutines needed to support the*  standard C library assert.**  October 1991*  Mike Westerfield**  Copyright 1991*  Byte Works, Inc.******************************************************************;-----------------------------------------------;;   Imported addresses;;-----------------------------------------------	IMPORT	abort	IMPORT	printf******************************************************************  void __assert (char *f, int l)**  Inputs:*        f - pointer to the file name*        l - line number******************************************************************		EXPORT	__assert __assert PROC;;       csubroutine (4:f,2:l),0	; f        equ   0+4+0l        equ   4+4+0         tsc         phd         tcd;;       ph2   l	;          LDA   l         PHA;;       ph4   f	;          LDA   f+2         PHA         LDA   f         PHA;;       ph4   #msg	;          PEA   +(msg)>>16         PEA   msg         jsl   printf         jsl   abort;;       creturn	;          lda   0+2         sta   0+6+2         lda   0+1         sta   0+6+1         pld         tsc         clc         adc   #0+6         tcs         rtlmsg	DC.B	'Assertion failed: file %s, line %d'	DC.B	10,0		ENDP		END