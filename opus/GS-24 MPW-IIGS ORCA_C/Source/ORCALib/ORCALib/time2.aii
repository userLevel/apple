			case  on	seg   ''******************************************************************  Time - Time and date libraries for C**  This code implements the tables and subroutines needed to*  support the standard C library TIME.**  January 1989*  Mike Westerfield**  Copyright 1989*  Byte Works, Inc.** Note: Portions of this library appear in SysFloat******************************************************************;-----------------------------------------------;;   Imported addresses;;-----------------------------------------------	IMPORT	~DIV4	IMPORT	~MOD4	IMPORT	~MUL4;-----------------------------------------------;;   Forward addresses and entries;;-----------------------------------------------	ENTRY	localtime	ENTRY	mktime	ENTRY	~ctime2******************************************************************  TimeCommon - common variables for the time library*******************************************************************TimeCommon privdataTimeCommon		PROC	;;  For conversion to/from seconds since 1970;		ENTRY	yearyear		DS.B 4                        ;year    0..99		ENTRY	monthmonth		DS.B 4                        ;month   1..12		ENTRY	dayday		DS.B 4                        ;day     1..31		ENTRY	hourhour		DS.B 4                        ;hour    0..23		ENTRY	minuteminute		DS.B 4                        ;minute  0..59		ENTRY	secondsecond		DS.B 4                        ;second  0..59		ENTRY	countcount		DS.B 4                        ;seconds since 1 Jan 1970		ENTRY	t1t1		DS.B 4                        ;work variable		ENTRY	t2t2		DS.B 4                        ;work variable		ENDP******************************************************************  char *asctime(struct tm *ts)**  Inputs:*        ts - time record to create string for**  Outputs:*        returns a pointer to the time string******************************************************************		EXPORT	asctime  asctime  PROC;;       csubroutine (4:ts),0	; ts       equ   0+4+0         tsc         phd         tcd         phb         phk         plb         brl   ~ctime2		ENDP******************************************************************  clock_t clock()**  Outputs:*        X-A - tick count******************************************************************		EXPORT	clock    clock    PROC         pha         pha;;       _GetTick         LDX   #$2503         JSL   $E10000         pla         plx         rtl		ENDP******************************************************************  char *ctime(timeptr)*        time_t *timptr;**  Inputs:*        timeptr - time to create string for**  Outputs:*        returns a pointer to the time string******************************************************************		EXPORT	ctime    ctime    PROCtm_sec   equ   0                        ;displacements into the time recordtm_min   equ   2tm_hour  equ   4tm_mday  equ   6tm_mon   equ   8tm_year  equ   10tm_wday  equ   12;;       csubroutine (4:timeptr),0	; timeptr  equ   0+4+0         tsc         phd         tcd         phb         phk         plb;;       ph4   timeptr                  ;convert to a time record	;          LDA   timeptr+2         PHA         LDA   timeptr         PHA         jsl   localtime         sta   timeptr         stx   timeptr+2		EXPORT	~ctime2~ctime2		;		         ldy   #tm_wday                 ;convert the week day to a string         lda   [timeptr],Y         asl   a         asl   a         tax         lda   weekDay,X         sta   str         lda   weekDay+1,X         sta   str+1         ldy   #tm_mon                  ;convert the month to a string         lda   [timeptr],Y         asl   a         asl   a         tax         lda   monthStr,X         sta   str+4         lda   monthStr+1,X         sta   str+5         ldy   #tm_mday                 ;convert the day to a string         lda   [timeptr],Y         jsr   mkstr         sta   str+8         ldy   #tm_hour                 ;convert the hour to a string         lda   [timeptr],Y         jsr   mkstr         sta   str+11         ldy   #tm_min                  ;convert minutes to a string         lda   [timeptr],Y         jsr   mkstr         sta   str+14         ldy   #tm_sec                  ;convert seconds to a string         lda   [timeptr],Y         jsr   mkstr         sta   str+17         ldy   #tm_year                 ;convert the year to a string         lda   #'91'         sta   str+20         lda   [timeptr],Y         cmp   #100         blt   lb1         ldx   #'02'         stx   str+20         sec         sbc   #100lb1      jsr   mkstr         sta   str+22;;       lla   timeptr,str	;          LDA   #str         STA   timeptr         LDA   #^str         STA   2+timeptr         plb;;       creturn 4:timeptr	;          ldx   timeptr+2         ldy   timeptr         lda   0+2         sta   0+4+2         lda   0+1         sta   0+4+1         pld         tsc         clc         adc   #0+4         tcs         tya         rtlweekDay	DC.B	'Sun Mon Tue Wed Thu Fri Sat'monthStr	DC.B	'Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec'str	DC.B	'Sun Jan 00 00:00:00 1900'	DC.B	10,0;;  mkstr - convert a number to a string;mkstr    ldx   #-1mk1      inx         sec         sbc   #10         bcs   mk1         clc         adc   #10         xba         pha         txa         ora   1,S         ora   #$3030         plx         rts		ENDP******************************************************************  factor - compute the seconds since 1 ;Jan 1970 from date**  Inputs:*        year,month,day,hour,minute,second - time to convert**  Outputs:*        count - seconds since 1 Jan 1970*******************************************************************factor   privatefactor		PROC	;using TimeCommon;;  compute the # of days since 1 Jan 1970;;;       mul4  year,#365,count          ;count := 365*year + day + 31*(month-1);;       ~SETM	; ;;       PH4   year	;          LDA   year+2         PHA         LDA   year         PHA;;       PH4   #365	;          PEA   +(365)>>16         PEA   365         JSL   ~MUL4;;       PL4   count	;          PLA         STA   count         PLA         STA   count+2;;       ~RESTM	; ;;       add4  count,day;;       ~SETM	;          CLC;;       ~LDA  count         LDA   count;;       ~OP   ADC,day         ADC   day;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H ADC,day	;          ADC   2+day;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       mul4  month,#31,t1;;       ~SETM	; ;;       PH4   month	;          LDA   month+2         PHA         LDA   month         PHA;;       PH4   #31	;          PEA   +(31)>>16         PEA   31         JSL   ~MUL4;;       PL4   t1	;          PLA         STA   t1         PLA         STA   t1+2;;       ~RESTM	; ;;       add4  count,t1;;       ~SETM	;          CLC;;       ~LDA  count         LDA   count;;       ~OP   ADC,t1         ADC   t1;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H ADC,t1	;          ADC   2+t1;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       sub4  count,#31;;       ~SETM	;          SEC;;       ~LDA  count         LDA   count;;       ~OP   SBC,#31         SBC   #31;;       ~STA  count         STA   count         BCS   ~32;;       ~OP.H DEC,count	;          DEC   2+count~32	; ;;       ~RESTM	; ;;       move4 year,t2                  ;t2 := year;;       ~SETM	;          LDA   2+year         STA   2+t2         LDA   year         STA   t2;;       ~RESTM	;          lda   month                    ;if January or February then         cmp   #3         bge   lb1         dec   t2                       ;  year := year-1         bra   lb2                      ;else;;lb1    mul4  month,#4,t1              ;  count := count - (month*4+23) div 10;;lb1    ~SETMlb1	; ;;       PH4   month	;          LDA   month+2         PHA         LDA   month         PHA;;       PH4   #4	;          PEA   +(4)>>16         PEA   4         JSL   ~MUL4;;       PL4   t1	;          PLA         STA   t1         PLA         STA   t1+2;;       ~RESTM	; ;;       add4  t1,#23;;       ~SETM	;          CLC;;       ~LDA  t1         LDA   t1;;       ~OP   ADC,#23         ADC   #23;;       ~STA  t1         STA   t1         BCC   ~48;;       ~OP.H INC,t1	;          INC   2+t1~48	; ;;       ~RESTM	; ;;       div4  t1,#10;;       ~SETM	; ;;       PH4   t1	;          LDA   t1+2         PHA         LDA   t1         PHA;;       PH4   #10	;          PEA   +(10)>>16         PEA   10         JSL   ~DIV4;;       PL4   t1	;          PLA         STA   t1         PLA         STA   t1+2         PLA         PLA;;       ~RESTM	; ;;       sub4  count,t1;;       ~SETM	;          SEC;;       ~LDA  count         LDA   count;;       ~OP   SBC,t1         SBC   t1;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H SBC,t1	;          SBC   2+t1;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; lb2      lda   t2                       ;count := count + year div 4         lsr   A         lsr   A         clc         adc   count         sta   count         bcc   lb3         inc   count+2;;lb3    add4  t2,#300                  ;count := count -;;lb3    ~SETMlb3	;          CLC;;       ~LDA  t2         LDA   t2;;       ~OP   ADC,#300         ADC   #300;;       ~STA  t2         STA   t2         BCC   ~70;;       ~OP.H INC,t2	;          INC   2+t2~70	; ;;       ~RESTM	; ;;       div4  t2,#100                  ;  ((300+year) div 100+1)*3 div 4;;       ~SETM	; ;;       PH4   t2	;          LDA   t2+2         PHA         LDA   t2         PHA;;       PH4   #100	;          PEA   +(100)>>16         PEA   100         JSL   ~DIV4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2         PLA         PLA;;       ~RESTM	; ;;       inc4  t2;;       ~SETM	;          INC   t2         BNE   ~83         INC   2+t2;;~83    ~RESTM~83	; ;;       mul4  t2,#3;;       ~SETM	; ;;       PH4   t2	;          LDA   t2+2         PHA         LDA   t2         PHA;;       PH4   #3	;          PEA   +(3)>>16         PEA   3         JSL   ~MUL4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2;;       ~RESTM	; ;;       div4  t2,#4;;       ~SETM	; ;;       PH4   t2	;          LDA   t2+2         PHA         LDA   t2         PHA;;       PH4   #4	;          PEA   +(4)>>16         PEA   4         JSL   ~DIV4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2         PLA         PLA;;       ~RESTM	; ;;       sub4  count,t2;;       ~SETM	;          SEC;;       ~LDA  count         LDA   count;;       ~OP   SBC,t2         SBC   t2;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H SBC,t2	;          SBC   2+t2;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       sub4  count,#25516             ;subtract off days between 1 Jan 00 and;;       ~SETM	;          SEC;;       ~LDA  count         LDA   count;;       ~OP   SBC,#25516         SBC   #25516;;       ~STA  count         STA   count         BCS   ~107;;       ~OP.H DEC,count	;          DEC   2+count~107	; ;;       ~RESTM	; *                                       ; 1 Jan 70;;  Convert to seconds and add in time of day in seconds;;;       mul4  count,#24*60*60          ;convert to seconds;;       ~SETM	; ;;       PH4   count	;          LDA   count+2         PHA         LDA   count         PHA;;       PH4   #24*60*60	;          PEA   +(24*60*60)>>16         PEA   $5180   ;60*60*24         JSL   ~MUL4;;       PL4   count	;          PLA         STA   count         PLA         STA   count+2;;       ~RESTM	; ;;       mul4  hour,#3600,t1            ;add in hours*3600;;       ~SETM	; ;;       PH4   hour	;          LDA   hour+2         PHA         LDA   hour         PHA;;       PH4   #3600	;          PEA   +(3600)>>16         PEA   3600         JSL   ~MUL4;;       PL4   t1	;          PLA         STA   t1         PLA         STA   t1+2;;       ~RESTM	; ;;       add4  count,t1;;       ~SETM	;          CLC;;       ~LDA  count         LDA   count;;       ~OP   ADC,t1         ADC   t1;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H ADC,t1	;          ADC   2+t1;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       mul4  minute,#60,t1            ;add in minutes*60;;       ~SETM	; ;;       PH4   minute	;          LDA   minute+2         PHA         LDA   minute         PHA;;       PH4   #60	;          PEA   +(60)>>16         PEA   60         JSL   ~MUL4;;       PL4   t1	;          PLA         STA   t1         PLA         STA   t1+2;;       ~RESTM	; ;;       add4  count,t1;;       ~SETM	;          CLC;;       ~LDA  count         LDA   count;;       ~OP   ADC,t1         ADC   t1;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H ADC,t1	;          ADC   2+t1;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       add4  count,second             ;add in seconds;;       ~SETM	;          CLC;;       ~LDA  count         LDA   count;;       ~OP   ADC,second         ADC   second;;       ~STA  count         STA   count;;       ~LDA.H count	;          LDA   2+count;;       ~OP.H ADC,second	;          ADC   2+second;;       ~STA.H count	;          STA   2+count;;       ~RESTM	;          rts		ENDP******************************************************************  struct tm *localtime(t)*        time_t *t;**  Inputs:*        t - # seconds since 1 Jan 1970**  Outputs:*        returns a pointer to a time record******************************************************************		EXPORT	localtime localtime PROC		EXPORT	gmtimegmtime		;			;using TimeCommon;;       csubroutine (4:t),0	; t        equ   0+4+0         tsc         phd         tcd         phb         phk         plb         ldy   #2                       ;dereference the pointer         lda   [t],Y         tax         lda   [t]         sta   t         stx   t+2         lda   #69                      ;find the year         sta   year         lda   #1         sta   month         sta   day         stz   hour         stz   minute         stz   secondlb1      inc   year         jsr   factor         lda   count+2         cmp   t+2         bne   lb1a         lda   count         cmp   t;;lb1a   ble   lb1lb1a     blt   lb1         beq   lb1         dec   yearlb2      inc   month                    ;find the month         jsr   factor         lda   count+2         cmp   t+2         bne   lb2a         lda   count         cmp   t;;lb2a   ble   lb2lb2a     blt   lb2         beq   lb2         dec   month         jsr   factor                   ;recompute the factor         lda   year                     ;set the year         sta   tm_year         lda   month                    ;set the month         dec   A         sta   tm_mon;;       sub4  t,count                  ;find the number of seconds;;       ~SETM	;          SEC;;       ~LDA  t         LDA   t;;       ~OP   SBC,count         SBC   count;;       ~STA  t         STA   t;;       ~LDA.H t	;          LDA   2+t;;       ~OP.H SBC,count	;          SBC   2+count;;       ~STA.H t	;          STA   2+t;;       ~RESTM	; ;;       move4 t,t1;;       ~SETM	;          LDA   2+t         STA   2+t1         LDA   t         STA   t1;;       ~RESTM	; ;;       div4  t,#60;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #60	;          PEA   +(60)>>16         PEA   60         JSL   ~DIV4;;       PL4   t	;          PLA         STA   t         PLA         STA   t+2         PLA         PLA;;       ~RESTM	; ;;       mul4  t,#60,t2;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #60	;          PEA   +(60)>>16         PEA   60         JSL   ~MUL4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2;;       ~RESTM	; ;;       sub4  t1,t2;;       ~SETM	;          SEC;;       ~LDA  t1         LDA   t1;;       ~OP   SBC,t2         SBC   t2;;       ~STA  t1         STA   t1;;       ~LDA.H t1	;          LDA   2+t1;;       ~OP.H SBC,t2	;          SBC   2+t2;;       ~STA.H t1	;          STA   2+t1;;       ~RESTM	;          lda   t1         sta   tm_sec;;       move4 t,t1                     ;find the number of minutes;;       ~SETM	;          LDA   2+t         STA   2+t1         LDA   t         STA   t1;;       ~RESTM	; ;;       div4  t,#60;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #60	;          PEA   +(60)>>16         PEA   60         JSL   ~DIV4;;       PL4   t	;          PLA         STA   t         PLA         STA   t+2         PLA         PLA;;       ~RESTM	; ;;       mul4  t,#60,t2;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #60	;          PEA   +(60)>>16         PEA   60         JSL   ~MUL4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2;;       ~RESTM	; ;;       sub4  t1,t2;;       ~SETM	;          SEC;;       ~LDA  t1         LDA   t1;;       ~OP   SBC,t2         SBC   t2;;       ~STA  t1         STA   t1;;       ~LDA.H t1	;          LDA   2+t1;;       ~OP.H SBC,t2	;          SBC   2+t2;;       ~STA.H t1	;          STA   2+t1;;       ~RESTM	;          lda   t1         sta   tm_min;;       move4 t,t1                     ;find the number of hours;;       ~SETM	;          LDA   2+t         STA   2+t1         LDA   t         STA   t1;;       ~RESTM	; ;;       div4  t,#24;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #24	;          PEA   +(24)>>16         PEA   24         JSL   ~DIV4;;       PL4   t	;          PLA         STA   t         PLA         STA   t+2         PLA         PLA;;       ~RESTM	; ;;       mul4  t,#24,t2;;       ~SETM	; ;;       PH4   t	;          LDA   t+2         PHA         LDA   t         PHA;;       PH4   #24	;          PEA   +(24)>>16         PEA   24         JSL   ~MUL4;;       PL4   t2	;          PLA         STA   t2         PLA         STA   t2+2;;       ~RESTM	; ;;       sub4  t1,t2;;       ~SETM	;          SEC;;       ~LDA  t1         LDA   t1;;       ~OP   SBC,t2         SBC   t2;;       ~STA  t1         STA   t1;;       ~LDA.H t1	;          LDA   2+t1;;       ~OP.H SBC,t2	;          SBC   2+t2;;       ~STA.H t1	;          STA   2+t1;;       ~RESTM	;          lda   t1         sta   tm_hour         lda   t                        ;set the day         inc   A         sta   tm_mday;;       ph4   #tm_sec                  ;set the day of week/year	;          PEA   +(tm_sec)>>16         PEA   tm_sec         jsl   mktime;;       lla   t,tm_sec	;          LDA   #tm_sec         STA   t         LDA   #^tm_sec         STA   2+t         plb;;       creturn 4:t	;          ldx   t+2         ldy   t         lda   0+2         sta   0+4+2         lda   0+1         sta   0+4+1         pld         tsc         clc         adc   #0+4         tcs         tya         rtltm_sec		DS.B 2                        ;seconds         0..59tm_min		DS.B 2                        ;minutes         0..59tm_hour		DS.B 2                        ;hours           0..23tm_mday		DS.B 2                        ;day             1..31tm_mon		DS.B 2                        ;month           0..11tm_year		DS.B 2                        ;year            70..200 (1900=0)tm_wday		DS.B 2                        ;day of week     0..6    (Sun = 0)tm_yday		DS.B 2                        ;day of year     0..365tm_isdst	DC.W	0	;not daylight savings time		ENDP******************************************************************  time_t mktime(tmptr)*        struct tm *tmptr**  Inputs:*        tmptr - poiner to a time record**  Outputs:*        tmptr->wday - day of week*        tmptr->yday - day of year*        returns the ime in seconds since 1 Jan 1970******************************************************************		EXPORT	mktime   mktime   PROC	;using TimeCommontemp     equ   1                        ;temp variabletemp2    equ   5                        ;temp variable;;       csubroutine (4:tmptr),8	; tmptr    equ   0+4+8         tsc         sec         sbc   #8         tcs         phd         tcd         phb         phk         plb;;       lla   temp,-1                  ;assume we can't do it	;          LDA   #-1         STA   temp         LDA   #^-1         STA   2+temp         ldy   #10                      ;error if year < 70         lda   [tmptr],Y         sta   year         cmp   #70;;       jlt   lb1         BGE   *+5         BRL   lb1         dey                            ;set the other time parameters         dey         lda   [tmptr],Y         inc   A         sta   month         dey         dey         lda   [tmptr],Y         sta   day         dey         dey         lda   [tmptr],Y         sta   hour         dey         dey         lda   [tmptr],Y         sta   minute         lda   [tmptr]         sta   second         jsr   factor                   ;compute seconds since 1970;;       move4 count,temp               ;save the value for later return;;       ~SETM	;          LDA   2+count         STA   2+temp         LDA   count         STA   temp;;       ~RESTM	;          lda   #1                       ;compute the days since the start of the         sta   month                    ; year         sta   day         jsr   factor;;       sub4  temp,count,count;;       ~SETM	;          SEC;;       ~LDA  temp         LDA   temp;;       ~OP   SBC,count         SBC   count;;       ~STA  count         STA   count;;       ~LDA.H temp	;          LDA   2+temp;;       ~OP.H SBC,count	;          SBC   2+count;;       ~STA.H count	;          STA   2+count;;       ~RESTM	; ;;       div4  count,#60*60*24;;       ~SETM	; ;;       PH4   count	;          LDA   count+2         PHA         LDA   count         PHA;;       PH4   #60*60*24	;          PEA   +(60*60*24)>>16         PEA   $5180   ;60*60*24         JSL   ~DIV4;;       PL4   count	;          PLA         STA   count         PLA         STA   count+2         PLA         PLA;;       ~RESTM	;          ldy   #14                      ;set the days         lda   count         inc   A         sta   [tmptr],Y;;       div4  temp,#60*60*24,temp2     ;compute the day of week;;       ~SETM	; ;;       PH4   temp	;          LDA   temp+2         PHA         LDA   temp         PHA;;       PH4   #60*60*24	;          PEA   +(60*60*24)>>16         PEA   $5180   ;60*60*24         JSL   ~DIV4;;       PL4   temp2	;          PLA         STA   temp2         PLA         STA   temp2+2         PLA         PLA;;       ~RESTM	; ;;       add4  temp2,#4;;       ~SETM	;          CLC;;       ~LDA  temp2         LDA   temp2;;       ~OP   ADC,#4         ADC   #4;;       ~STA  temp2         STA   temp2         BCC   ~29;;       ~OP.H INC,temp2	;          INC   2+temp2~29	; ;;       ~RESTM	; ;;       mod4  temp2,#7;;       ~SETM	; ;;       PH4   temp2	;          LDA   temp2+2         PHA         LDA   temp2         PHA;;       PH4   #7	;          PEA   +(7)>>16         PEA   7         JSL   ~MOD4         PLA         PLA;;       PL4   temp2	;          PLA         STA   temp2         PLA         STA   temp2+2;;       ~RESTM	;          lda   temp2                    ;set the day of week         ldy   #12         sta   [tmptr],Ylb1      plb;;       creturn 4:temp	;          ldx   temp+2         ldy   temp         lda   8+2         sta   8+4+2         lda   8+1         sta   8+4+1         pld         tsc         clc         adc   #8+4         tcs         tya         rtl		ENDP******************************************************************  time_t time(tptr)*        time_t *tptr;**  Outputs:*        tptr - if non-null, the value it points to is set*        time - returns the value******************************************************************		EXPORT	time     time     PROC	;using TimeCommon;;       csubroutine (4:tptr),0	; tptr     equ   0+4+0         tsc         phd         tcd         phb         phk         plb;;  get the current time;         pha                            ;get the current time         pha         pha         pha;;       _ReadTimeHex         LDX   #$0D03         JSL   $E10000         lda   5,S                      ;set the day         and   #$00FF         inc   A         sta   day         lda   5,S                      ;set the month         and   #$FF00         xba         inc   A         sta   month         lda   3,S                      ;set the year         and   #$FF00         xba         sta   year         lda   3,S                      ;set the hour         and   #$00FF         sta   hour         lda   1,S                      ;set the minute         xba         and   #$00FF         sta   minute         pla                            ;set the second         and   #$00FF         sta   second         pla                            ;clean up the stack         pla         pla         jsr   factor                   ;convert the seconds         lda   tptr                     ;if tptr <> nil then         ora   tptr+2         beq   lb1         ldy   #2                       ;  place the result there         lda   count         sta   [tptr]         lda   count+2         sta   [tptr],Y;;lb1    move4 count,tptr;;lb1    ~SETMlb1	;          LDA   2+count         STA   2+tptr         LDA   count         STA   tptr;;       ~RESTM	;          plb;;       creturn 4:tptr	;          ldx   tptr+2         ldy   tptr         lda   0+2         sta   0+4+2         lda   0+1         sta   0+4+1         pld         tsc         clc         adc   #0+4         tcs         tya         rtl		ENDP		END